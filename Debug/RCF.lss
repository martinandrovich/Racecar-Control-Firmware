                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > CONSTANTS
                                 
                                 	.EQU	MSB			= 7														; Most Signifigant Bit
                                 
                                 	.EQU	BAUDRATE	= 0x00CF												; Baudrate configuration (default = 0xCF)
                                 
                                 	.EQU	TMR1FREQ	= 976 - 1												; Timer1 configuration
                                 
                                 																				; 62500 - 1		= 4Hz
                                 																				; 31250 - 1		= 8Hz
                                 																				; 15625 - 1		= 16Hz
                                 																				;  7812 - 1		= 32Hz
                                 																				;  1953 - 1		= 128Hz
                                 																				;   976 - 1		= 256Hz [DEFAULT]
                                 																				;   488 - 1		= 512Hz
                                 																				;	244 - 1		= 1024Hz
                                 																				;	122 - 1		= 2048Hz
                                 																				;	 61 - 1		= 4096Hz
                                 
                                 	; Moving Average Filter Constants
                                 	
                                 	.EQU	MOVAVG_SIZE					= 64									; Size (bytes) of Moving Average Filter
                                 	.EQU	MOVAVG_DIVS					= 6										; Number of division to perform (i.e. 2^5 = 32)
                                 	.EQU	MOVAVG_TABLE_END			= MOVAVG_TABLE + MOVAVG_SIZE			;
                                 
                                 	; Mapping & Turn Detection Constants
                                 
                                 	.EQU	TURN_TH_IN_LEFT				= 122
                                 	.EQU	TURN_TH_IN_RIGHT			= 115
                                 	.EQU	TURN_TH_OUT_LEFT			= 122
                                 	.EQU	TURN_TH_OUT_RIGHT			= 115
                                 
                                 	.EQU	MAPPING_SEEK_PWM			= 65									; Mapping Seek PWM in BYTES (0-255)
                                 	.EQU	MAPPING_PWM					= 90									; Mapping PWM in BYTES (0-255)
                                 	.EQU	MAPPING_DEBOUNCE_VAL		= 10									; Mapping Debounce in TICKS
                                 	.EQU	MAPPING_OFFSET_IN			= 8										; Mapping Offset In in TICKS
                                 	.EQU	MAPPING_OFFSET_OUT			= 11									; Mapping Offset Out in TICKS
                                 
                                 	; Trajectory Constants
                                 	
                                 	.EQU	TRAJECTORY_ACCLR_OFFSET		= 0										;
                                 	.EQU	TRAJECTORY_BRAKE_TOLERANCE	= 5										;
                                 
                                 	.EQU	TRAJECTORY_ACCLR_PWM		= 200									;
                                 	.EQU	TRAJECTORY_TURN_PWM			= 101									;
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > REGISTERS
                                 
                                 	.DEF	MDFLG		= R0													; Mode Flags
                                 	.DEF	FNFLG		= R1													; Function Flags for Interrupts
                                 	.DEF	MTFLG		= R2													; Mapping & Trajectory Flags
                                 	
                                 	.DEF	TEMP1		= R16													; Temporary Register #1
                                 	.DEF	TEMP2		= R17													; Temporary Register #2
                                 	.DEF	TEMP3		= R18													; Temporary Register #3
                                 	.DEF	TEMPI		= R19													; Temporary Interrupts Register
                                 	
                                 	.DEF	TEMPWH		= R25													; Temporary Register (Word) Pair
                                 	.DEF	TEMPWL		= R24													; ^
                                 		
                                 	.DEF	RXREG		= R20													; USART Reception Register
                                 	.DEF	TXREG		= R21													; USART Transmission Register
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > FLAGS
                                 
                                 	; MDFLG | Mode Flags 
                                 
                                 	.EQU	AUTO		= 7														; Autonomous Mode
                                 	.EQU	MAP			= 6														; Mapping Mode
                                 	.EQU	BROD2		= 5														; Broadcast Mode
                                 	.EQU	BROD1		= 4														; ^
                                 	.EQU	BROD0		= 3														; ^
                                 
                                 	; FNFLG | Function Flags
                                 
                                 	.EQU	TACHO		= 7														; Tachometer Ready
                                 	.EQU	FNLNE		= 6														; Finishline Ready
                                 	.EQU	ACCLR		= 5														; Accelerometer Ready
                                 	.EQU	TMR1		= 4														; Timer1 Ready
                                 	.EQU	CMDPD		= 3														; Command Pending
                                 
                                 	; MTFLG | Mapping & Trajectory Flags
                                 
                                 	.EQU	ISMAP		= 7														; Program currently mapping
                                 	.EQU	INTURN		= 6														; In turn
                                 	.EQU	TURNDIR		= 5														; Direction of turn (0 = L & 1 = R)
                                 	.EQU	MPRDY		= 4														; Mapping is ready
                                 	.EQU	ISRUN		= 3														; Trajectory is running
                                 	.EQU	ISBRK		= 2														; Braking is active
                                 
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> INITIALIZATION
                                 
                                 INIT:
                                 
                                 	; Stack Pointer
                                 
000051 e008                      	LDI 	TEMP1, HIGH(RAMEND)													; Initialize Stack Pointer
000052 bf0e                      	OUT 	SPH, TEMP1															; ^
000053 e50f                      	LDI 	TEMP1,  LOW(RAMEND)													; ^
000054 bf0d                      	OUT 	SPL, TEMP1															; ^
                                 
                                 	; SRAM Initialization
                                 
000055 2700                      	CLR		TEMP1																; Initialize allocated SRAM to NULL
000056 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^
000058 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
00005a 9300 006f                 	STS		MODE_FLG, TEMP1														; ^
00005c 9300 006e                 	STS		FUNC_FLG, TEMP1														; ^
00005e 9300 0060                 	STS		TACHOMETER_H, TEMP1													; ^
000060 9300 0061                 	STS		TACHOMETER_L, TEMP1													; ^
000062 9300 0076                 	STS		TACHOMETER_L_PREV, TEMP1											; ^
000064 9300 0074                 	STS		TURN_MIN_TACHOMETER_H, TEMP1										; ^
000066 9300 0075                 	STS		TURN_MIN_TACHOMETER_L, TEMP1										; ^
000068 9300 0073                 	STS		ACCELEROMETER, TEMP1												; ^
00006a 9300 0064                 	STS		ADC_H, TEMP1														; ^
00006c 9300 0065                 	STS		ADC_L, TEMP1														; ^
00006e 9300 0070                 	STS		FINISHLINE, TEMP1
                                 
000070 940e 0318                 	CALL	MOVAVG_POINTER_RESET												; Reset Moving Average Pointer
000072 940e 031b                 	CALL	MOVAVG_SRAM_SETUP													; Initialize allocated Moving Average SRAM to default
                                 
                                 	; Flags Initialization
                                 
000074 2400                      	CLR		MDFLG																; Clear Flag Registers
000075 2411                      	CLR		FNFLG																; ^
000076 2422                      	CLR		MTFLG																; ^
                                 
                                 	; USART Config
                                 
000077 e000                      	LDI		TEMP1, HIGH(BAUDRATE)												; Set Transmission Rate
000078 bd00                      	OUT		UBRRH, TEMP1														; ^
000079 ec0f                      	LDI		TEMP1,  LOW(BAUDRATE)												; ^
00007a b909                      	OUT		UBRRL, TEMP1														; ^
                                 
00007b e002                      	LDI		TEMP1, 0x02															; Clear all Error Flags + Enable DoubleMode
00007c b90b                      	OUT		UCSRA, TEMP1														; ^
                                 
00007d e108                      	LDI		TEMP1, (1<<RXEN)|(1<<TXEN)											; Enable Transmission & Reception
00007e b90a                      	OUT		UCSRB, TEMP1														; ^
                                 
00007f e806                      	LDI		TEMP1, (1<<URSEL)|(3<<UCSZ0)										; Set Frame Format (8, N, 1)
000080 bd00                      	OUT		UCSRC, TEMP1														; ^
                                 
000081 2744                      	CLR		RXREG																; Reset Reception Register
000082 2755                      	CLR		TXREG																; Reset Transmission Register
                                 
                                 	; ADC Config	
                                 
000083 e200                      	LDI		TEMP1, (1<<ADLAR)													; Choose -> ADC0 and AVCC. Vcc = 5V
000084 b907                      	OUT		ADMUX, TEMP1														; AUTOTRIGGER ENABLED (ADATE) otherwise it doesnt work?
                                 
000085 e80f                      	LDI		TEMP1, (1<<ADEN)|(1<<ADIE)|(1<<ADPS2)|(1<<ADPS1)|(1<<ADPS0)			; ADEN: ENABLE ADC, ADSC: START CONVERSATION ; (1<<ADPS2)
000086 b906                      	OUT		ADCSRA, TEMP1														; ADFR: Activate Free Running Select, Prescaler: 128 // 125kHz ADC clock
                                 
000087 9a36                      	SBI		ADCSR, ADSC															; Start ADC Conversion	
                                 
                                 	; I/O (Port) Setup
                                 
000088 9a8c                      	SBI		DDRD, PD4															; Set PD4 on PORTD as Output
000089 9a8f                      	SBI		DDRD, PD7															; Set PD7 on PORTD as Output
00008a 9a92                      	SBI		PORTD, PD2															; Set PD2 on PORTD as Pullup Input
                                 
00008b 9894                      	CBI		PORTD, PD4															; Disable MOSFET Brake
                                 
                                 	; Timer1 Setup
                                 
00008c e100                      	LDI		TEMP1, (1<<OCIE1A)													; Enable Timer1 Compare Match Interrupt
00008d bf09                      	OUT		TIMSK, TEMP1														; ^
                                 
00008e e000                      	LDI		TEMP1, 0x00															; Set Default
00008f bd0f                      	OUT		TCCR1A, TEMP1														; ^
                                 
000090 e00b                      	LDI		TEMP1, (1<<CS11)|(1<<CS10)|(1<<WGM12)								; Set 64 Prescelar, CTC-MODE
000091 bd0e                      	OUT		TCCR1B, TEMP1														; ^
                                 
000092 e003                      	LDI		TEMP1, HIGH(TMR1FREQ)												; Set timer offset
000093 bd0b                      	OUT		OCR1AH, TEMP1														; ^
000094 ec0f                      	LDI		TEMP1,  LOW(TMR1FREQ)												; ^
000095 bd0a                      	OUT		OCR1AL, TEMP1														; ^
                                 
000096 e004                      	LDI		TEMP1, (1<<TOV1)													; Enable Timer1
000097 bf08                      	OUT		TIFR, TEMP1															; ^
                                 	
                                 	; Waveform Generator (Timer2)
                                 
000098 e000                      	LDI		TEMP1, 0x00															; Reset Timer2
000099 bd03                      	OUT		OCR2, TEMP1															; ^
                                 
                                 	; External Interrupt Setup
                                 	
00009a e00f                      	LDI		TEMP1, (1<<ISC01)|(1<<ISC00) | (1<<ISC10)|(1<<ISC11)				; Set INT0 & INT1 to rising edge
00009b bf05                      	OUT		MCUCR, TEMP1														; ^
                                 
00009c ec00                      	LDI 	TEMP1, (1<<INT0)|(1<<INT1)											; Enable external interrupts
00009d bf0b                      	OUT 	GICR, TEMP1															; ^
                                 
00009e 9478                      	SEI																			; Set Global Interrupt Flag
                                 
00009f c000                      	RJMP	MAIN																; Start MAIN Program
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> MAIN PROGRAM
                                 
                                 MAIN:
                                 	
0000a0 940e 0337                 	CALL	LOAD_FLAGS															; Load Flags
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > FUNCTIONS
                                 	
0000a2 940e 027c                 	CALL	TELEGRAM_CHECK														; Check for Telegrams
                                 
0000a4 fc13                      	SBRC	FNFLG, CMDPD														; Command Pending
0000a5 940e 02b5                 	CALL	EXECUTE_COMMAND														; ^
                                 
0000a7 fc16                      	SBRC	FNFLG, FNLNE														; Finishline Ready
0000a8 940e 00cd                 	CALL	LOG_FINISHLINE														; ^
                                 	
0000aa fc17                      	SBRC	FNFLG, TACHO														; Tachometer Ready
0000ab 940e 00c0                 	CALL	LOG_TACHOMETER														; ^
                                 
0000ad fc15                      	SBRC	FNFLG, ACCLR														; Accelerometer Ready
0000ae 940e 00dd                 	CALL	LOG_ACCELEROMETER													; ^
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MODES
                                 
0000b0 fc07                      	SBRC	MDFLG, AUTO															; Autonomous Mode
0000b1 940e 00f2                 	CALL	AUTONOMOUS															; ^
                                 
0000b3 fc23                      	SBRC	MTFLG, ISRUN														; Trajectory Mode
0000b4 940e 01e8                 	CALL	TRAJECTORY_RUN														; ^
                                 
0000b6 fc06                      	SBRC	MDFLG, MAP															; Mapping Mode
0000b7 940e 010e                 	CALL	MAPPING																; ^
                                 
0000b9 fc03                      	SBRC	MDFLG, BROD0														; Broadcast Mode
0000ba 940e 0247                 	CALL	BROADCAST															; ^
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > CLOCK (TIMER1)
                                 
0000bc fc14                      	SBRC	FNFLG, TMR1															; Timer1 Ready
0000bd 940e 0340                 	CALL	CLOCK																; ^
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > REPEAT LOOP
                                 
0000bf cfe0                      	RJMP	MAIN																; Loop forever
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> SENSOR PROCESSING & LOGGING
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > TACHOMETER
                                 
                                 LOG_TACHOMETER:
                                 	
                                 	// Tachometer value is not allowed to exceed 32.768
                                 	
0000c0 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load previous values from SRAM
0000c2 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; into WORD registers
                                 
0000c4 9601                      	ADIW	TEMPWH:TEMPWL, 1													; Increment Tachometer value
                                 
0000c5 9390 0060                 	STS		TACHOMETER_H, TEMPWH												; Store new values into SRAM
0000c7 9380 0061                 	STS		TACHOMETER_L, TEMPWL												; ^
                                 
0000c9 2d01
0000ca 770f
0000cb 2e10                      	CFLG	FNFLG, TACHO														; Clear TACHO flag in FNFLG
                                 
0000cc 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > FINISH LINE
                                 
                                 LOG_FINISHLINE:
                                 	
0000cd 9100 0070                 	LDS		TEMP1, FINISHLINE													; Load, increment & store Finishline value
0000cf 9503                      	INC		TEMP1																; ^
0000d0 9300 0070                 	STS		FINISHLINE, TEMP1													; ^
                                 
0000d2 2700                      	CLR		TEMP1																; Reset Tachometer
0000d3 9300 0060                 	STS		TACHOMETER_H, TEMP1													; ^
0000d5 9300 0061                 	STS		TACHOMETER_L, TEMP1													; ^
                                 
0000d7 fc06                      	SBRC	MDFLG, MAP	 														; Skip clearing flag if Mapping Mode enabled
0000d8 9508                      	RET																			; ^
                                 
0000d9 2d01
0000da 7b0f
0000db 2e10                      	CFLG	FNFLG, FNLNE														; Clear FNLNE flag in FNFLG
                                 
0000dc 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > ACCELEROMETER
                                 
                                 LOG_ACCELEROMETER:
                                 
0000dd fe14                      	SBRS	FNFLG, TMR1															; Check if logging is synchronized with CLOCK (Timer1)
0000de 9508                      	RET
                                 
0000df fc23                      	SBRC	MTFLG, ISRUN 														; Skip if Trajectory is running
0000e0 9508                      	RET																			; ^
                                 
0000e1 b104                      	IN		TEMP1, ADCL															; Read LOW of ADC
0000e2 0000                      	NOP																			; ^
0000e3 9300 0065                 	STS		ADC_L, TEMP1														; ^
                                 
0000e5 b105                      	IN		TEMP1, ADCH															; Read HIGH of ADC
0000e6 0000                      	NOP																			; ^
0000e7 9300 0064                 	STS		ADC_H, TEMP1														; ^
                                 
0000e9 9300 0073                 	STS		ACCELEROMETER, TEMP1
                                 
0000eb 940e 0309                 	CALL	MOVAVG																; Apply Moving Average Filter
                                 
0000ed 9a36                      	SBI		ADCSR, ADSC															; Start ADC Conversion
                                 
0000ee 2d01
0000ef 7d0f
0000f0 2e10                      	CFLG	FNFLG, ACCLR														; Clear ACCLR flag in FNFLG
                                 
0000f1 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> AUTONOMOUS MODE
                                 
                                 AUTONOMOUS:
                                 
0000f2 fc23                      	SBRC	MTFLG, ISRUN														; Escape if Trajectory is Running
0000f3 c019                      	RJMP	AUTONOMOUS_ESC														; ^
                                 
0000f4 fe06                      	SBRS	MDFLG, MAP															; Begin Mapping if not Mapping
0000f5 c003                      	RJMP	AUTONOMOUS_BEGIN_MAPPING											; ^
                                 
0000f6 fe24                      	SBRS	MTFLG, MPRDY														; Escape if Mapping not Ready
0000f7 c015                      	RJMP	AUTONOMOUS_ESC														; ^
                                 
0000f8 c00b                      	RJMP	AUTONOMOUS_RUN														; Start Trajectory
                                 
                                 AUTONOMOUS_BEGIN_MAPPING:
                                 
0000f9 fc24                      	SBRC	MTFLG, MPRDY														; Start Trajectory if Mapping already done
0000fa c009                      	RJMP	AUTONOMOUS_RUN														; ^
                                 	
0000fb 2d00
0000fc 6400
0000fd 2e00                      	SFLG	MDFLG, MAP															; Set MAPP flag in MDFLG
                                 
0000fe e401                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Start vehicle with mapping seek PWM
0000ff 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
000101 940e 02f3                 	CALL	SET_MOTOR_PWM														; ^
                                 
000103 c009                      	RJMP	AUTONOMOUS_ESC														; Escape
                                 
                                 AUTONOMOUS_RUN:
                                 
                                 	// Can be disabled in order to only recompile mapping after each Run
                                 
                                 	;CFLG	MTFLG, MPRDY														; Clear MPRDY flag in MTFLG
                                 
000104 e401                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Set Motor Speed to Sleep PWM
000105 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^
000107 940e 02f3                 	CALL	SET_MOTOR_PWM														; ^
                                 
000109 940e 0185                 	CALL	TRAJECTORY_COMPILE													; Compile Trajectory
00010b 940e 01e8                 	CALL	TRAJECTORY_RUN														; Start Trajectory
                                 
                                 AUTONOMOUS_ESC:
                                 
00010d 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> MAPPING
                                 
                                 MAPPING:
                                 	
00010e fc16                      	SBRC	FNFLG, FNLNE														; Check FNLNE flag
00010f c003                      	RJMP	MAPPING_ISMAP_CHECK													; If SET then check ISMAP
                                 
000110 fc27                      	SBRC	MTFLG, ISMAP														; Check ISMAP flag
000111 c008                      	RJMP	MAPPING_DEBOUNCE_CHECK												; If SET then continue mapping
                                 
000112 c071                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_ISMAP_CHECK:
                                 
                                 	// If  ISMAP -> CLR ISMAP(MTFLG) && CLR MAP(MDFLG)							= End Mapping
                                 	// If ~ISMAP -> SET ISMAP													= Begin Mapping
                                 
000113 2d01
000114 7b0f
000115 2e10                      	CFLG	FNFLG, FNLNE														; Clear FNFNE flag in FNFLG
                                 	
000116 fc27                      	SBRC	MTFLG, ISMAP														; Check ISMAP flag
000117 c02a                      	RJMP	MAPPING_END															; If SET then end mapping
                                 
