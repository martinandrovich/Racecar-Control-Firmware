                                 ; >> TRAJECTORY
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMPILER
                                 TRAJECTORY_COMPILE:
                                 TRAJECTORY_COMPILER_SETUP:
                                 
000185 e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
000186 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 
000187 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
000188 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
000189 9109                      	LD		TEMP1, Y+															; Load first Mapping value
00018a 9119                      	LD		TEMP2, Y+															; ^
                                 
00018b 3000                      	CPI		TEMP1, 0															; Perfrom RUNUP if first Mapping value is 0
00018c f009                      	BREQ	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
00018d 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_RUNUP:
                                 
00018e 9109                      	LD		TEMP1, Y+															; Load Mapping value
00018f 9119                      	LD		TEMP2, Y+															; ^
                                 
000190 3f0f                      	CPI		TEMP1, 0xFF															; Loop until EoT is found
000191 f7e1                      	BRNE	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
000192 9724                      	SBIW	Y, 4																; Offset the mapping to last outgoing turn in Mapping
                                 
000193 9109                      	LD		TEMP1, Y+															; Load Mapping values
000194 8118                      	LD		TEMP2, Y															; ^
                                 
000195 930d                      	ST		X+, TEMP1															; Store values into Trajectory SRAM
000196 931d                      	ST		X+, TEMP2															; ^
                                 
000197 9190 0078                 	LDS		TEMPWH, TRACK_LENGTH_H 												; Load Track Length
000199 9180 0079                 	LDS		TEMPWL, TRACK_LENGTH_L												; ^
                                 
00019b 1b81                      	SUB		TEMPWL, TEMP2														; Subtract Track Length from Last Turn value (negative expected)
00019c 0b90                      	SBC		TEMPWH, TEMP1														; ^
                                 
00019d 9310 007c                 	STS		LATEST_STRAIGHT_L, TEMP2											; Store Latest Straight into SRAM
00019f 9300 007b                 	STS		LATEST_STRAIGHT_H, TEMP1											; ^
                                 
0001a1 e0d2                      	LDI		YH, HIGH(MAPP_TABLE+2)												; Load Y Pointer to (offset) Mapping Table
0001a2 e0c2                      	LDI		YL,  LOW(MAPP_TABLE+2)												; ^
                                 
0001a3 9119                      	LD		TEMP2, Y+															
0001a4 9109                      	LD		TEMP1, Y+
                                 
0001a5 9190 007b                 	LDS		TEMPWH, LATEST_STRAIGHT_H											; Load Latest Straight
0001a7 9180 007c                 	LDS		TEMPWL,	LATEST_STRAIGHT_L											; ^
                                 
0001a9 771f                      	CBR		TEMP2, (1<<MSB)
                                 
0001aa 0f08                      	ADD		TEMP1, TEMPWL
0001ab 1f19                      	ADC		TEMP2, TEMPWH
                                 
0001ac 9300 007a                 	STS		LATEST_STRAIGHT, TEMP1
                                 
0001ae d027                      	RCALL	TRAJECTORY_COMPILER_BREAK_OFFSET									; Find Brake Offset
                                 
0001af e0d2                      	LDI		YH, HIGH(MAPP_TABLE+4)												; Load Y Pointer to (offset) Mapping Table
0001b0 e0c4                      	LDI		YL,  LOW(MAPP_TABLE+4)												; ^
                                 
0001b1 c01a                      	RJMP	JUMPTO
                                 
                                 
                                 TRAJECTORY_COMPILER_LOOP:
                                 
0001b2 9119                      	LD		TEMP2, Y+															; Load Mapping values
0001b3 9109                      	LD		TEMP1, Y+															; ^
                                 
0001b4 3f1f                      	CPI		TEMP2, 0xFF															; Check EoT
0001b5 f171                      	BREQ	TRAJECTORY_COMPILER_END												; ^
                                 
0001b6 fd17                      	SBRC	TEMP2, MSB															; Check MSB:
0001b7 c00a                      	RJMP	TRAJECTORY_COMPILER_BREAK											; if 1 then Break
0001b8 c000                      	RJMP	TRAJECTORY_COMPILER_ACCELERATE										; if 0 then Accelerate
                                 	
                                 
                                 TRAJECTORY_COMPILER_ACCELERATE:
                                 	
0001b9 01c8                      	MOVW	TEMPWH:TEMPWL, TEMP2:TEMP1											; 
0001ba 9700                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_ACCLR_OFFSET								; Subtract Acceleration Offset
                                 
0001bb 939d                      	ST		X+, TEMPWH															; Store into Trajectory SRAM
0001bc 938d                      	ST		X+, TEMPWL															; ^
                                 
0001bd 9310 007b                 	STS		LATEST_STRAIGHT_H, TEMP2											; Store Latest Straight into SRAM
0001bf 9300 007c                 	STS		LATEST_STRAIGHT_L, TEMP1											; ^
                                 
0001c1 cff0                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK:
                                 	
0001c2 9190 007b                 	LDS		TEMPWH, LATEST_STRAIGHT_H											; Load Latest Straight
0001c4 9180 007c                 	LDS		TEMPWL,	LATEST_STRAIGHT_L											; ^
                                 
0001c6 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 	
0001c7 1b08                      	SUB		TEMP1, TEMPWL														; Calculate Latest Straight
0001c8 0b19                      	SBC		TEMP2, TEMPWH														; ^
                                 
0001c9 9300 007a                 	STS		LATEST_STRAIGHT, TEMP1												; Store Latest Straight value into SRAM
                                 
                                 	;MOV		TXREG, TEMP2
                                 	;CALL	SERIAL_WRITE
                                 
                                 	;MOV		TXREG, TEMP1
                                 	;CALL	SERIAL_WRITE
                                 
0001cb d00a                      	RCALL	TRAJECTORY_COMPILER_BREAK_OFFSET									; Find Brake Offset
                                 JUMPTO:
0001cc 9722                      	SBIW	YH:YL, 2															; Reload Mapping values
0001cd 9119                      	LD		TEMP2, Y+															; ^
0001ce 9109                      	LD		TEMP1, Y+															; ^
                                 
0001cf 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 
0001d0 1b02                      	SUB		TEMP1, TEMP3														; Subtract Brake Offset
0001d1 4010                      	SBCI	TEMP2, 0															; ^
                                 
0001d2 6810                      	SBR		TEMP2, (1<<MSB)														; Set MSB
                                 
0001d3 931d                      	ST		X+, TEMP2															; Store into Trajectory SRAM
0001d4 930d                      	ST		X+,	TEMP1															; ^
                                 
0001d5 cfdc                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET:
                                 	
0001d6 e0f0                      	LDI		ZH, HIGH(BREAK_OFFSET_TABLE*2)										; Load Z Pointer to Break Offset Table
0001d7 e8e8                      	LDI		ZL,  LOW(BREAK_OFFSET_TABLE*2)										; ^
                                 
0001d8 9100 007a                 	LDS		TEMP1, LATEST_STRAIGHT												; Load value of Latest Straight Distance
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP:
                                 
0001da 9114                      	LPM		TEMP2, Z															; Load value of Z Pointer from Table and increment by 2
0001db 9632                      	ADIW	ZH:ZL, 2
                                 
0001dc 1710                      	CP		TEMP2, TEMP1														; Find matching Table value
0001dd f408                      	BRSH	TRAJECTORY_COMPILER_BREAK_OFFSET_END								; ^
                                 
0001de cffb                      	RJMP	TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_END:
                                 
0001df 9731                      	SBIW	ZH:ZL, 1															; Decrement Z Pointer
0001e0 9124                      	LPM		TEMP3, Z															; Load value of Z Pointer															
                                 
0001e1 940e 02a6                 	CALL	TELEGRAM_RESET														; Reset Telegram due to Z Pointer
                                 
0001e3 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_END:
                                 
0001e4 ef0f                      	SER		TEMP1																; Store 0xFFFF into trajectory in SRAM
0001e5 930d                      	ST		X+, TEMP1															; ^
0001e6 930c                      	ST		X, TEMP1															; ^
                                 
0001e7 9508                      	RET																			; Return	
                                 
