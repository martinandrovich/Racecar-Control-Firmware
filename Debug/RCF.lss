0001ea 910c                      	LD		TEMP1, X															; Check EoT
0001eb 3f0f                      	CPI		TEMP1, 0xFF															; ^
0001ec f159                      	BREQ	TRAJECTORY_RUN_END													; ^
                                 	
0001ed 9100 0060                 	LDS		TEMP1, TACHOMETER_H													; Load Tachometer values
0001ef 9110 0061                 	LDS		TEMP2, TACHOMETER_L													; ^
                                 	
0001f1 fc22                      	SBRC	MTFLG, ISBRK														; Check ISBRK FLAG
0001f2 c010                      	RJMP	TRAJECTORY_RUN_CHECK_ISBRK											;
0001f3 c018                      	RJMP	TRAJECTORY_RUN_CHECK_MSB											;
                                 
                                 TRAJECTORY_RUN_SETUP:
                                 	
0001f4 e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
0001f5 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 	
0001f6 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
0001f7 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
0001f8 910d                      	LD		TEMP1, X+															; Shitty Quick Fix
0001f9 911d                      	LD		TEMP2, X+															; ^
                                 
0001fa 9712                      	SBIW	XH:XL, 2															; ^
                                 			
0001fb 9300 007d                 	STS		LAST_TURN_H, TEMP1													; ^
