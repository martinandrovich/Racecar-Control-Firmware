000114 2d02
000115 6400
000116 2e20                      	SFLG	MDFLG, MAP															; Set MAPP flag in MDFLG
                                 
000117 e40e                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Start vehicle with mapping seek PWM
000118 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
00011a 940e 0327                 	CALL	SET_MOTOR_PWM														; ^
                                 
00011c c009                      	RJMP	AUTONOMOUS_ESC														; Escape
                                 
