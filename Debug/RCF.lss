00013a e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
00013b 2700                      	CLR		TEMP1																; Store 0x0000 into mapping in SRAM
00013c 9309                      	ST		Y+, TEMP1															; ^
00013d 9309                      	ST		Y+, TEMP1															; ^
                                 
00013e c042                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_END:
                                 
00013f 2d00
000140 7b0f
000141 2e00                      	CFLG	MDFLG, MAP															; Clear MAP flag in MDFLG
000142 2d02
000143 770f
000144 2e20                      	CFLG	MTFLG, ISMAP														; Clear ISMAP flag in MTFLG
000145 2d02
000146 6100
000147 2e20                      	SFLG	MTFLG, MPRDY														; Set MPRDY flag in MTFLG
                                 
000148 9110 0060                 	LDS		TEMP2, TACHOMETER_H													; Load current Tachometer values
00014a 9120 0061                 	LDS		TEMP3, TACHOMETER_L													; ^
                                 
00014c 9310 0078                 	STS		TRACK_LENGTH_H, TEMP2 												; Store Track Length (Finishline Tachometer value) into SRAM
00014e 9320 0079                 	STS		TRACK_LENGTH_L, TEMP3 												; ^
                                 
                                 	;ST		Y+, TEMP2															; Save finishline tachometer data
                                 	;ST		Y+, TEMP3															;
                                 
000150 ef0f                      	SER		TEMP1																; Store 0xFFFF into mapping in SRAM
000151 9309                      	ST		Y+, TEMP1															; ^
000152 8308                      	ST		Y, TEMP1															; ^
                                 
000153 fe07                      	SBRS	MDFLG, AUTO															; Skip if in Autonomous Mode
000154 940e 02d6                 	CALL	SET_MOTOR_BREAK														; Break vehicle
                                 
000156 c02a                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN:
                                 
000157 9110 0073                 	LDS		TEMP2, ACCELEROMETER												; Load Accelerometer value
                                 
000159 fc26                      	SBRC	MTFLG, INTURN														; Check INTURN flag
00015a c00c                      	RJMP	MAPPING_CHECK_TURN_OUT												; If SET then check if a turn has been exited
00015b c000                      	RJMP	MAPPING_CHECK_TURN_IN												; If CLR then check if a turn has been detected
                                 
                                 MAPPING_CHECK_TURN_IN:
                                 
                                 	; Check Right Turn
                                 
00015c 2d02
00015d 6200
00015e 2e20                      	SFLG	MTFLG, TURNDIR														; Set TURNDIR flag in MTFLG
                                 
00015f 3713                      	CPI		TEMP2, TURN_TH_IN_RIGHT												; Check Right Turn In
000160 f078                      	BRLO	MAPPING_ADD															; Create mapping entry if true
                                 
                                 	; Check Left Turn
                                 
000161 2d02
000162 7d0f
000163 2e20                      	CFLG	MTFLG, TURNDIR														; Clear TURNDIR flag in MTFLG
                                 
000164 371a                      	CPI		TEMP2, TURN_TH_IN_LEFT												; Check Left Turn In
000165 f450                      	BRSH	MAPPING_ADD															; Create mapping entry if true
                                 
000166 c01a                      	RJMP	MAPPING_ESC															; Return
                                 
