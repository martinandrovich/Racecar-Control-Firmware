0000fd 9300 0064                 	STS		ADC_H, TEMP1														; ^
                                 
0000ff 9300 0073                 	STS		ACCELEROMETER, TEMP1
                                 
000101 940e 034e                 	CALL	MOVAVG																; Apply Moving Average Filter
                                 
