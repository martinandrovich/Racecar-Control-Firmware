000114 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
000116 940e 02f9                 	CALL	SET_MOTOR_PWM														; ^
                                 
000118 c009                      	RJMP	AUTONOMOUS_ESC														; Escape
                                 
