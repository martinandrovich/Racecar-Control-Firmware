00002f b907                      	OUT		ADMUX, TEMP1																		; AUTOTRIGGER ENABLED (ADATE) otherwise it doesnt work?
000030 e80f                      	LDI		TEMP1, (1<<ADEN)|(1<<ADIE)|(1<<ADPS1)|(1<<ADPS0)|(1<<ADPS2)							;(1<<ADATE)	; ADEN: ENABLE ADC, ADSC: START CONVERSATION ; (1<<ADPS2)
000031 b906                      	OUT		ADCSRA, TEMP1																		; ADFR: Activate Free Running Select, Prescalar: 128 // 125kHz ADC clock // *might be divided by 2*
                                 
