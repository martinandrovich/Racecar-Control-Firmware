000082 0000                      	NOP										; No code yet
000083 9508                      	RET										; Return
                                 
                                 
                                 SERIAL_READ:
000084 9b5f                      	SBIS	UCSRA, RXC						; Wait for Recieve (RXC) flag
000085 cffe                      	RJMP	SERIAL_READ						; ^
                                 
000086 b14c                      	IN		RXREG, UDR						; Load data from serial to register
                                 
000087 9508                      	RET										; Return
                                 
                                 
