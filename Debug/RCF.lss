000170 2d02                      	MOV		TEMP1, MTFLG														; Toggle INTURN flag
000171 e410                      	LDI		TEMP2, (1<<INTURN)													; ^
000172 2701                      	EOR		TEMP1, TEMP2														; ^
000173 2e20                      	MOV		MTFLG, TEMP1														; ^
                                 
000174 dfae                      	RCALL	MAPPING_DEBOUNCE_RESET												; Reset Tachometer Debounce
                                 
000175 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer values
000177 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 
000179 fc26                      	SBRC	MTFLG, INTURN														; 
00017a 9708                      	SBIW	TEMPWH:TEMPWL, MAPPING_OFFSET_IN									;
00017b fe26                      	SBRS	MTFLG, INTURN														;
00017c 970b                      	SBIW	TEMPWH:TEMPWL, MAPPING_OFFSET_OUT									;
                                 
                                 	// Tachometer value is not allowed to exceed 32.768
                                 
00017d fd06                      	SBRC	TEMP1, INTURN														; Set MSB of Tachometer (HIGH) to value of INTURN bit
00017e 6890                      	ORI		TEMPWH, (1<<7)														; ^
                                 	
00017f 9399                      	ST		Y+, TEMPWH															; Store mapping into SRAM
000180 9389                      	ST		Y+, TEMPWL															; ^
                                 
