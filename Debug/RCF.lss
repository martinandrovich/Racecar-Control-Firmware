00033b 9508                      	RET
                                 
                                 TEST40:
                                 	
00033c e450                      	LDI		TXREG, 0x40
00033d 940e 025d                 	CALL	SERIAL_WRITE
                                 	
00033f 9508                      	RET
                                 
                                 TEST45:
                                 	
000340 e455                      	LDI		TXREG, 0x45
000341 940e 025d                 	CALL	SERIAL_WRITE
                                 	
000343 9508                      	RET
                                 
                                 DELAY:
000344 e604                          LDI		TEMP1, 100
                                 LOOP3:
