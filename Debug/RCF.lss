00003e 9a8f                      	SBI		DDRD, PD7									; Set PIN7 on PORTD as Output
00003f 9a92                      	SBI		PORTD, PD2									; Set PIN2 on PORTD as Pullup Input
                                 
                                 	; Interrupt setup
                                 
000040 e003                      	LDI		TEMP1, (1<<ISC01) | (1<<ISC00)				; Set INT0 to rising edge
000041 bf05                      	OUT		MCUCR, TEMP1								; ^
                                 
000042 e400                      	LDI 	TEMP1, (1<<INT0)							; Enable external interrupts
000043 bf0b                      	OUT 	GICR, TEMP1									; ^
                                 
