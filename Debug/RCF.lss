                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> MAPPING
                                 
                                 MAPPING:
                                 	
000123 fc36                      	SBRC	FNFLG, FNLNE														; Check FNLNE flag
000124 c003                      	RJMP	MAPPING_ISMAP_CHECK													; If 1 then check ISMAP
                                 
000125 fc47                      	SBRC	MTFLG, ISMAP														; Check ISMAP flag
000126 c008                      	RJMP	MAPPING_DEBOUNCE_CHECK												; If 1 then continue Mapping
                                 
000127 c076                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_ISMAP_CHECK:
                                 
000128 2d03
000129 7b0f
00012a 2e30                      	CFLG	FNFLG, FNLNE														; Clear FNFNE flag in FNFLG
                                 	
00012b fc47                      	SBRC	MTFLG, ISMAP														; Check ISMAP flag
00012c c02a                      	RJMP	MAPPING_END															; If 1 then end Mapping
                                 
00012d fe47                      	SBRS	MTFLG, ISMAP														; Check ISMAP flag
00012e c016                      	RJMP	MAPPING_BEGIN														; If 0 then begin Mapping
                                 
                                 MAPPING_DEBOUNCE_CHECK:
                                 	
00012f 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer value
000131 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 
000133 9110 0074                 	LDS		TEMP2, MAPPING_DEBOUNCE_H											; Load Debounce Tachometer value
000135 9120 0075                 	LDS		TEMP3, MAPPING_DEBOUNCE_L											; ^
                                 
000137 1782                      	CP		TEMPWL, TEMP3														; Check if Debounce is reached
000138 0791                      	CPC		TEMPWH, TEMP2 														; ^
000139 f5d0                      	BRSH	MAPPING_CHECK_TURN													; ^
                                 	
00013a 9508                      	RET																			; Return
                                 
                                 MAPPING_DEBOUNCE_RESET:
                                 
00013b 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer value
00013d 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 	
00013f 960a                      	ADIW	TEMPWH:TEMPWL, MAPPING_DEBOUNCE_VAL									; Update Minumum Tachometer Detection value (current Tachometer + Constant)
                                 
000140 9390 0074                 	STS		MAPPING_DEBOUNCE_H, TEMPWH											; Store values to SRAM
000142 9380 0075                 	STS		MAPPING_DEBOUNCE_L, TEMPWL											; ^
                                 
000144 9508                      	RET																			; Return
                                 	
                                 MAPPING_BEGIN:
                                 
000145 2d04
000146 7e0f
000147 2e40                      	CFLG	MTFLG, MPRDY														; Clear MPRDY flag in MTFLG
000148 2d04
000149 6800
00014a 2e40                      	SFLG	MTFLG, ISMAP														; Set ISMAP flag in MTFLG
                                 
00014b dfef                      	RCALL	MAPPING_DEBOUNCE_RESET												; Reset Tachometer Debounce											
                                 
00014c e50a                      	LDI		TEMP1, MAPPING_PWM													; Start vehicle with mapping PWM
00014d 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
00014f 940e 02f9                 	CALL	SET_MOTOR_PWM														; ^
                                 
000151 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Initialize Y Pointer
000152 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
000153 2700                      	CLR		TEMP1																; Store 0x0000 into mapping in SRAM
000154 9309                      	ST		Y+, TEMP1															; ^
000155 9309                      	ST		Y+, TEMP1															; ^
                                 
000156 c047                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_END:
                                 
000157 9110 0060                 	LDS		TEMP2, TACHOMETER_H													; Load current Tachometer values
000159 9120 0061                 	LDS		TEMP3, TACHOMETER_L													; ^
                                 
00015b 9310 0078                 	STS		TRACK_LENGTH_H, TEMP2 												; Store Track Length (Finishline Tachometer value) into SRAM
00015d 9320 0079                 	STS		TRACK_LENGTH_L, TEMP3 												; ^
                                 
                                 	;ST		Y+, TEMP2															; Save finishline tachometer data
                                 	;ST		Y+, TEMP3															;
                                 
00015f ef0f                      	SER		TEMP1																; Store 0xFFFF into mapping in SRAM
000160 9309                      	ST		Y+, TEMP1															; ^
000161 8308                      	ST		Y, TEMP1															; ^
                                 
000162 fe27                      	SBRS	MDFLG, AUTO															; Skip if in Autonomous Mode
000163 940e 0306                 	CALL	SET_MOTOR_BREAK														; Break vehicle
                                 
000165 2d02
000166 7b0f
000167 2e20                      	CFLG	MDFLG, MAP															; Clear MAP flag in MDFLG
000168 2d04
000169 770f
00016a 2e40                      	CFLG	MTFLG, ISMAP														; Clear ISMAP flag in MTFLG
00016b 2d04
00016c 6100
00016d 2e40                      	SFLG	MTFLG, MPRDY														; Set MPRDY flag in MTFLG
                                 
00016e 2700                      	CLR		TEMP1																; Reset Tachometer
00016f 9300 0060                 	STS		TACHOMETER_H, TEMP1													; ^
000171 9300 0061                 	STS		TACHOMETER_L, TEMP1													; ^
                                 
000173 c02a                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN:
                                 
000174 9110 0073                 	LDS		TEMP2, ACCELEROMETER												; Load Accelerometer value
                                 
000176 fc46                      	SBRC	MTFLG, INTURN														; Check INTURN flag
000177 c00c                      	RJMP	MAPPING_CHECK_TURN_OUT												; If 1 then check if a turn has been exited
000178 c000                      	RJMP	MAPPING_CHECK_TURN_IN												; If 0 then check if a turn has been detected
                                 
                                 MAPPING_CHECK_TURN_IN:
                                 
                                 	; Check Right Turn
                                 
000179 2d04
00017a 6200
00017b 2e40                      	SFLG	MTFLG, TURNDIR														; Set TURNDIR flag in MTFLG
                                 
00017c 3713                      	CPI		TEMP2, TURN_TH_IN_RIGHT												; Check Right Turn In
00017d f078                      	BRLO	MAPPING_ADD															; Create Mapping entry if true
                                 
                                 	; Check Left Turn
                                 
00017e 2d04
00017f 7d0f
000180 2e40                      	CFLG	MTFLG, TURNDIR														; Clear TURNDIR flag in MTFLG
                                 
000181 371a                      	CPI		TEMP2, TURN_TH_IN_LEFT												; Check Left Turn In
000182 f450                      	BRSH	MAPPING_ADD															; Create mapping entry if true
                                 
000183 c01a                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN_OUT:
                                 
000184 fc45                      	SBRC	MTFLG, TURNDIR														; Check TURNDIR flag:
000185 c001                      	RJMP	MAPPING_CHECK_TURN_OUT_RIGHT										; If 1 then check RIGHT turn out
000186 c003                      	RJMP	MAPPING_CHECK_TURN_OUT_LEFT											; If 0 then check LEFT turn out
                                 
                                 MAPPING_CHECK_TURN_OUT_RIGHT:
                                 
000187 3713                      	CPI		TEMP2, TURN_TH_OUT_RIGHT											; Check Right Turn Out
000188 f420                      	BRSH	MAPPING_ADD															; Create Mapping entry if true
                                 
000189 c014                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN_OUT_LEFT:
                                 
00018a 371a                      	CPI		TEMP2, TURN_TH_OUT_LEFT												; Check Left Turn Out
00018b f008                      	BRLO	MAPPING_ADD															; Create mapping entry if true
                                 
00018c c011                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_ADD:
                                 
00018d 2d04                      	MOV		TEMP1, MTFLG														; Toggle INTURN flag
00018e e410                      	LDI		TEMP2, (1<<INTURN)													; ^
00018f 2701                      	EOR		TEMP1, TEMP2														; ^
000190 2e40                      	MOV		MTFLG, TEMP1														; ^
                                 
000191 dfa9                      	RCALL	MAPPING_DEBOUNCE_RESET												; Reset Tachometer Debounce
                                 
000192 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer values
000194 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 
000196 fc46                      	SBRC	MTFLG, INTURN														; Calculate Offset (In/Out)
000197 9708                      	SBIW	TEMPWH:TEMPWL, MAPPING_OFFSET_IN									; ^
000198 fe46                      	SBRS	MTFLG, INTURN														; ^
000199 970b                      	SBIW	TEMPWH:TEMPWL, MAPPING_OFFSET_OUT									; ^
                                 
00019a fd06                      	SBRC	TEMP1, INTURN														; Set MSB of Tachometer (HIGH) to value of INTURN bit
00019b 6890                      	ORI		TEMPWH, (1<<7)														; ^
                                 	
00019c 9399                      	ST		Y+, TEMPWH															; Store mapping into SRAM
00019d 9389                      	ST		Y+, TEMPWL															; ^
                                 
                                 MAPPING_ESC:
                                 
00019e 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> TRAJECTORY
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMPILER
                                 TRAJECTORY_COMPILE:
                                 TRAJECTORY_COMPILER_SETUP:
                                 
00019f e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
0001a0 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 
0001a1 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
0001a2 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
0001a3 9109                      	LD		TEMP1, Y+															; Load first Mapping value
0001a4 9119                      	LD		TEMP2, Y+															; ^
                                 
0001a5 3000                      	CPI		TEMP1, 0															; Perfrom RUNUP if first Mapping value is 0
0001a6 f009                      	BREQ	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
0001a7 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_RUNUP:
                                 
0001a8 9109                      	LD		TEMP1, Y+															; Load Mapping value
0001a9 9119                      	LD		TEMP2, Y+															; ^
                                 
0001aa 3f0f                      	CPI		TEMP1, 0xFF															; Loop until EoT is found
0001ab f7e1                      	BRNE	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
0001ac 9724                      	SBIW	YH:YL, 4															; Offset the mapping to last outgoing turn in Mapping
                                 
0001ad 9109                      	LD		TEMP1, Y+															; Load Mapping values
0001ae 8118                      	LD		TEMP2, Y															; ^
                                 
0001af 930d                      	ST		X+, TEMP1															; Store values into Trajectory SRAM
0001b0 931d                      	ST		X+, TEMP2															; ^
                                 
0001b1 9300 007d                 	STS		LAST_TURN_H, TEMP1													; Store Last Turn into SRAM
0001b3 9310 007e                 	STS		LAST_TURN_L, TEMP2													; ^
                                 
0001b5 9190 0078                 	LDS		TEMPWH, TRACK_LENGTH_H 												; Load Track Length
0001b7 9180 0079                 	LDS		TEMPWL, TRACK_LENGTH_L												; ^
                                 
0001b9 1b18                      	SUB		TEMP2, TEMPWL														; Subtract Track Length from Last Turn value (negative expected)
0001ba 0b09                      	SBC		TEMP1, TEMPWH														; ^
                                 
0001bb 9310 007c                 	STS		LATEST_STRAIGHT_L, TEMP2											; Store Latest Straight into SRAM
0001bd 9300 007b                 	STS		LATEST_STRAIGHT_H, TEMP1											; ^
                                 
0001bf e0d2                      	LDI		YH, HIGH(MAPP_TABLE+2)												; Load Y Pointer to (offset) Mapping Table
0001c0 e0c2                      	LDI		YL,  LOW(MAPP_TABLE+2)												; ^
                                 
                                 TRAJECTORY_COMPILER_LOOP:
                                 
0001c1 9119                      	LD		TEMP2, Y+															; Load Mapping values
0001c2 9109                      	LD		TEMP1, Y+															; ^
                                 
0001c3 3f1f                      	CPI		TEMP2, 0xFF															; Check EoT
0001c4 f171                      	BREQ	TRAJECTORY_COMPILER_END												; ^
                                 
0001c5 fd17                      	SBRC	TEMP2, MSB															; Check MSB:
0001c6 c00a                      	RJMP	TRAJECTORY_COMPILER_BREAK											; if 1 then Break
0001c7 c000                      	RJMP	TRAJECTORY_COMPILER_ACCELERATE										; if 0 then Accelerate
                                 	
                                 
                                 TRAJECTORY_COMPILER_ACCELERATE:
                                 	
0001c8 01c8                      	MOVW	TEMPWH:TEMPWL, TEMP2:TEMP1											; 
0001c9 9700                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_ACCLR_OFFSET								; Subtract Acceleration Offset
                                 
0001ca 939d                      	ST		X+, TEMPWH															; Store into Trajectory SRAM
0001cb 938d                      	ST		X+, TEMPWL															; ^
                                 
0001cc 9310 007b                 	STS		LATEST_STRAIGHT_H, TEMP2											; Store Latest Straight into SRAM
0001ce 9300 007c                 	STS		LATEST_STRAIGHT_L, TEMP1											; ^
                                 
0001d0 cff0                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK:
                                 	
0001d1 9190 007b                 	LDS		TEMPWH, LATEST_STRAIGHT_H											; Load Latest Straight
0001d3 9180 007c                 	LDS		TEMPWL,	LATEST_STRAIGHT_L											; ^
                                 
0001d5 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 	
0001d6 1b08                      	SUB		TEMP1, TEMPWL														; Calculate Latest Straight
0001d7 0b19                      	SBC		TEMP2, TEMPWH														; ^
                                 
0001d8 9300 007a                 	STS		LATEST_STRAIGHT, TEMP1												; Store Latest Straight value into SRAM
                                 
0001da d00a                      	RCALL	TRAJECTORY_COMPILER_BREAK_OFFSET									; Find Brake Offset
                                 
0001db 9722                      	SBIW	YH:YL, 2															; Reload Mapping values
0001dc 9119                      	LD		TEMP2, Y+															; ^
0001dd 9109                      	LD		TEMP1, Y+															; ^
                                 
0001de 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 
                                 	// !#!#!#!
                                 	// Needs to account for negative remainder!
                                 
0001df 1b02                      	SUB		TEMP1, TEMP3														; Subtract Brake Offset
0001e0 4010                      	SBCI	TEMP2, 0															; ^
                                 
0001e1 6810                      	SBR		TEMP2, (1<<MSB)														; Set MSB
                                 
0001e2 931d                      	ST		X+, TEMP2															; Store into Trajectory SRAM
0001e3 930d                      	ST		X+,	TEMP1															; ^
                                 
0001e4 cfdc                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET:
                                 	
0001e5 e0f0                      	LDI		ZH, HIGH(BREAK_OFFSET_TABLE*2)										; Load Z Pointer to Break Offset Table
0001e6 e8ec                      	LDI		ZL,  LOW(BREAK_OFFSET_TABLE*2)										; ^
                                 
0001e7 9100 007a                 	LDS		TEMP1, LATEST_STRAIGHT												; Load value of Latest Straight Distance
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP:
                                 
0001e9 9114                      	LPM		TEMP2, Z															; Load value of Z Pointer from Table and increment by 2
0001ea 9632                      	ADIW	ZH:ZL, 2
                                 
0001eb 1710                      	CP		TEMP2, TEMP1														; Find matching Table value
0001ec f408                      	BRSH	TRAJECTORY_COMPILER_BREAK_OFFSET_END								; ^
                                 
0001ed cffb                      	RJMP	TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP								; Loop until found
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_END:
                                 
0001ee 9731                      	SBIW	ZH:ZL, 1															; Decrement Z Pointer
0001ef 9124                      	LPM		TEMP3, Z															; Load value of Z Pointer															
                                 
0001f0 940e 02a8                 	CALL	TELEGRAM_RESET														; Reset Telegram due to Z Pointer
                                 
0001f2 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_END:
                                 
0001f3 ef0f                      	SER		TEMP1																; Store 0xFFFF into trajectory in SRAM
0001f4 930d                      	ST		X+, TEMP1															; ^
0001f5 930c                      	ST		X, TEMP1															; ^
                                 
0001f6 9508                      	RET																			; Return	
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > RUNNER
                                 
                                 TRAJECTORY_RUN:
                                 
0001f7 fe43                      	SBRS	MTFLG, ISRUN														; Check if Trajectory Runner has been initalized
0001f8 d00a                      	RCALL	TRAJECTORY_RUN_SETUP												; and initialize if not.
                                 	
0001f9 910c                      	LD		TEMP1, X															; Check EoT
0001fa 3f0f                      	CPI		TEMP1, 0xFF															; ^
0001fb f121                      	BREQ	TRAJECTORY_RUN_END													; ^
                                 	
0001fc 9100 0060                 	LDS		TEMP1, TACHOMETER_H													; Load Tachometer values
0001fe 9110 0061                 	LDS		TEMP2, TACHOMETER_L													; ^
                                 	
000200 fc42                      	SBRC	MTFLG, ISBRK														; Check ISBRK flag:
000201 c009                      	RJMP	TRAJECTORY_RUN_CHECK_ISBRK											; if 1 then check breaking status
000202 c011                      	RJMP	TRAJECTORY_RUN_CHECK_MSB											; if 0 then check next MSB
                                 
                                 TRAJECTORY_RUN_SETUP:
                                 	
000203 e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
000204 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 	
000205 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
000206 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
000207 2d04
000208 6008
000209 2e40                      	SFLG	MTFLG, ISRUN														; Set ISRUN flag in MTFLG
                                 
00020a 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_CHECK_ISBRK:
                                 
00020b 9199                      	LD		TEMPWH, Y+															; Load Mapping Tachometer values
00020c 9189                      	LD		TEMPWL, Y+															; ^
                                 
00020d 9722                      	SBIW	YH:YL, 2															; Undo incrementation of Y Pointer
                                 
00020e 779f                      	CBR		TEMPWH, (1<<MSB)													; Clear MSB for Calculations
                                 	
00020f 9705                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_BRAKE_TOLERANCE							; Subtract Tolerance value
                                 
000210 1718                      	CP		TEMP2, TEMPWL														; Check if Current Tachometer >= Mapping Tachometer w/ Tolerance
000211 0709                      	CPC		TEMP1, TEMPWH														; ^
000212 f550                      	BRSH	TRAJECTORY_RUN_TURN													; ^	
                                 
000213 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_CHECK_MSB:
                                 
000214 919d                      	LD		TEMPWH, X+															; Load Trajectory Tachometer values
000215 918d                      	LD		TEMPWL, X+															; ^
                                 
000216 9712                      	SBIW	XH:XL, 2															; Undo incrementation of X Pointer
                                 
000217 e820                      	LDI		TEMP3, (1<<MSB)														; Extract & Remove MSB of Trajectory Tachometer value
000218 2329                      	AND		TEMP3, TEMPWH														; ^
000219 779f                      	CBR		TEMPWH, (1<<MSB)													; ^
                                 
00021a 1718                      	CP		TEMP2, TEMPWL														; Escape if Current Tachometer < Trajectory Tachometer
00021b 0709                      	CPC		TEMP1, TEMPWH														; ^ 
00021c f158                      	BRLO	TRAJECTORY_RUN_ESC													; ^
                                 
00021d fd27                      	SBRC	TEMP3, MSB															; Check MSB:
00021e c009                      	RJMP	TRAJECTORY_RUN_BREAK												; if 1 then Break
00021f c015                      	RJMP	TRAJECTORY_RUN_ACCELERATE											; if 0 then Accelerate
                                 
                                 TRAJECTORY_RUN_END:
                                 
000220 2d04
000221 7f07
000222 2e40                      	CFLG	MTFLG, ISRUN														; Clear ISRUN in MTFLG
                                 	
000223 940e 031e                 	CALL	MOVAVG_POINTER_RESET												; Reset Moving Average Pointer
000225 940e 0321                 	CALL	MOVAVG_SRAM_SETUP													; Initialize allocated Moving Average SRAM to default
                                 
000227 9508                      	RET
                                 
                                 TRAJECTORY_RUN_BREAK:
                                 
000228 9190 007d                 	LDS		TEMPWH, LAST_TURN_H													; Load value of Last Turn
00022a 9180 007e                 	LDS		TEMPWL, LAST_TURN_L													; ^
                                 
00022c 1718                      	CP		TEMP2, TEMPWL														; Escape if Current Tachometer >= Last Turn
00022d 0709                      	CPC		TEMP1, TEMPWH														; ^ 
00022e f4c8                      	BRSH	TRAJECTORY_RUN_ESC													; ^
                                 	
00022f 2d04
000230 6004
000231 2e40                      	SFLG	MTFLG, ISBRK														; Set ISBRK in MTFLG
                                 	
000232 940e 0306                 	CALL	SET_MOTOR_BREAK														; Set Motor to Brake PWM
                                 
000234 9508                      	RET																			; Return		
                                 
                                 TRAJECTORY_RUN_ACCELERATE:
                                 	
000235 ec28                      	LDI		TEMP3, TRAJECTORY_ACCLR_PWM											; Set Motor to Acceleration PWM
000236 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000238 940e 02f9                 	CALL	SET_MOTOR_PWM														; ^
                                 
00023a 9612                      	ADIW	XH:XL, 2															; Increment Pointers
00023b 9622                      	ADIW	YH:YL, 2															; ^
                                 
00023c 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_TURN:
                                 	
00023d 2d04
00023e 7f0b
00023f 2e40                      	CFLG	MTFLG, ISBRK														; Clear ISBRK flag in MTFLG
                                 
000240 e625                      	LDI		TEMP3, TRAJECTORY_TURN_PWM											; Set Motor to Turn PWM
000241 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000243 940e 02f9                 	CALL	SET_MOTOR_PWM														; ^
                                 
000245 9612                      	ADIW	XH:XL, 2															; Increment Pointers
000246 9622                      	ADIW	YH:YL, 2															; ^
                                 
000247 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_ESC:
                                 	
000248 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> BROADCAST
                                 
                                 BROADCAST:
                                 	
000249 fe34                      	SBRS	FNFLG, TMR1															; Check if broadcast is synchronized with CLOCK (Timer1)
00024a 9508                      	RET																			; ^
                                 	
00024b e300                      	LDI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Mask Broadcast Modes
00024c 2102                      	AND		TEMP1, MDFLG														; ^
                                 
00024d 3100                      	CPI		TEMP1, (1<<BROD1)													; Tachometer Mode		= (011)
00024e f031                      	BREQ	BROADCAST_TACHOMETER												; ^
                                 	
00024f 3300                      	CPI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Finishline Mode		= (111)
000250 f091                      	BREQ	BROADCAST_FINISHLINE												; ^
                                 
000251 3200                      	CPI		TEMP1, (1<<BROD2)													; Accelerometer Mode	= (101)
000252 f059                      	BREQ	BROADCAST_ACCELEROMETER												; ^
                                 
000253 d014                      	RCALL	BROADCAST_ALL														; Broadcast All			= (001)
                                 
000254 9508                      	RET																			; Return
                                 
                                 BROADCAST_TACHOMETER:
                                 
000255 9150 0060                 	LDS		TXREG, TACHOMETER_H													; Load & transmit HIGH byte of Tachometer data
000257 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
000259 9150 0061                 	LDS		TXREG, TACHOMETER_L													; Load & transmit HIGH byte of Tachometer data
00025b 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
00025d 9508                      	RET																			; Return
                                 
                                 BROADCAST_ACCELEROMETER:
                                 
00025e 9150 0073                 	LDS		TXREG, ACCELEROMETER												; Load & transmit Accelerometer data
000260 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
000262 9508                      	RET																			; Return
                                 
                                 BROADCAST_FINISHLINE:
                                 	
000263 9150 0070                 	LDS		TXREG, FINISHLINE													; Load & transmit Finishline data
000265 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
000267 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL:
                                 
000268 9100 0061                 	LDS		TEMP1, TACHOMETER_L													; Load current & recent Tachometer (LOW) value
00026a 9110 0076                 	LDS		TEMP2, TACHOMETER_L_PREV											;
                                 
00026c 1701                      	CP		TEMP1, TEMP2														; Compare and only transmit if value has changed
00026d f409                      	BRNE	BROADCAST_ALL_SEND													; ^
                                 
00026e 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL_SEND:
                                 
00026f 9300 0076                 	STS		TACHOMETER_L_PREV, TEMP1											; Update recent Tachometer (LOW) value
                                 
000271 dfe3                      	RCALL	BROADCAST_TACHOMETER												; Broadcast Tachometer
000272 dfeb                      	RCALL	BROADCAST_ACCELEROMETER												; Broadcast Accelerometer
                                 
000273 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> COMMUNICATION PROTOCOL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > USART
                                 
                                 SERIAL_READ:
                                 	
000274 9b5f                      	SBIS	UCSRA, RXC															; Wait for Recieve (RXC) Flag
000275 cffe                      	RJMP	SERIAL_READ															; ^
                                 
000276 b14c                      	IN		RXREG, UDR															; Read data into Reception Register
000277 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
000279 9508                      	RET																			; Return
                                 
                                 SERIAL_WRITE:
                                 	
00027a 9b5d                      	SBIS	UCSRA, UDRE															; Wait for Empty Transmit Buffer (UDRE) Flag
00027b cffe                      	RJMP	SERIAL_WRITE														; ^
                                 
00027c b95c                      	OUT		UDR, TXREG															; Write data from Transmission Register
                                 
00027d 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > TELEGRAM PARSER
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support recent Z pointer from SRAM!
                                 
                                 TELEGRAM_CHECK:
                                 
00027e 9b5f                      	SBIS	UCSRA, RXC															; Return if no data in USART reception buffer
00027f 9508                      	RET																			; ^
                                 
000280 b14c                      	IN		RXREG, UDR															; Read data into Reception Register
000281 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
                                 TELEGRAM_PARSER:
                                 	
000283 9100 0066                 	LDS		TEMP1, TEL_STEP														; Load, increment & store Telegram Step Counter
000285 9503                      	INC		TEMP1																; ^
000286 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
000288 3001                      	CPI		TEMP1, 1															; Setup Telegram Parser if Step = 1
000289 f041                      	BREQ	TELEGRAM_PARSE_SETUP												; ^
                                 
00028a 3004                      	CPI		TEMP1, 4															; Execute Telegram if Step = 4
00028b f089                      	BREQ	TELEGRAM_EXECUTE													; ^
                                 
00028c 3002                      	CPI		TEMP1, 2															; Parse (TYPE) if Step = 2
00028d f041                      	BREQ	TELEGRAM_JUMP														; ^
                                 
00028e 95ea                      	DEC		ZL																	; Offset Z pointer (-1) to parse TYPE (0x00_XX)
                                 
00028f 3003                      	CPI		TEMP1, 3															; Parse (COMMAND) if Step = 3
000290 f029                      	BREQ	TELEGRAM_JUMP														; ^
                                 
                                 TELEGRAM_PARSER_ESC:
                                 	
000291 9508                      	RET
                                 
                                 TELEGRAM_PARSE_SETUP:
                                 
000292 e0f0                      	LDI		ZH, HIGH(COMMANDS*2)												; Reset Z Pointer to COMMANDS jump table
000293 e5e0                      	LDI 	ZL,  LOW(COMMANDS*2)												; ^
                                 
000294 95e3                      	INC		ZL																	; Offset Z pointer (+1) to parse COMMAND (0xXX_00)
                                 
000295 cfed                      	RJMP	TELEGRAM_PARSER														; Return
                                 
                                 TELEGRAM_JUMP:
                                 
000296 9634                      	ADIW	ZH:ZL, 4															; Increment Z Pointer (by 4)
                                 
000297 9104                      	LPM		TEMP1, Z															; Load the matching duty cycle
                                 
000298 3e0e                      	CPI		TEMP1, 0xEE															; Reset everything if out of table bounds
000299 f0b1                      	BREQ	TELEGRAM_ERROR														; ^
                                 
00029a 1704                      	CP		TEMP1, RXREG														; Find match in jump table
00029b f3a9                      	BREQ	TELEGRAM_PARSER_ESC													; ^
                                 
00029c cff9                      	RJMP	TELEGRAM_JUMP														; Repeat
                                 
                                 TELEGRAM_EXECUTE:
                                 
00029d 9632                      	ADIW	ZH:ZL, 2															; Point at & read LOW of Table address
00029e 9104                      	LPM		TEMP1, Z															; ^
                                 
00029f 9631                      	ADIW	ZH:ZL, 1															; Point at & read HIGH of Table address
0002a0 9114                      	LPM		TEMP2, Z															; ^
                                 
0002a1 2fe0                      	MOV		ZL, TEMP1															; Load Z Pointer
0002a2 2ff1                      	MOV		ZH, TEMP2															; ^
                                 
0002a3 9340 0068                 	STS		RECENT_DAT, RXREG													; Store recieved data in SRAM
                                 
0002a5 2d03
0002a6 6008
0002a7 2e30                      	SFLG	FNFLG, CMDPD														; Set CMDPD flag in FNFLG
                                 
                                 TELEGRAM_RESET:
                                 	
0002a8 2700                      	CLR		TEMP1																; Reset & store parse step counter
0002a9 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
0002ab 9508                      	RET																			; Return
                                 
                                 TELEGRAM_CLRBUFFER:
                                 
0002ac b10c                      	IN		TEMP1, UDR															; Empty buffer
0002ad 995f                      	SBIC	UCSRA, RXC															; ^
0002ae cffd                      	RJMP	TELEGRAM_CLRBUFFER													; ^
                                 
0002af 9508                      	RET																			; Return
                                 
                                 TELEGRAM_ERROR:
                                 
0002b0 2744                      	CLR		RXREG																; Clear reception register
                                 
0002b1 2d03
0002b2 7f07
0002b3 2e30                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
0002b4 dff3                      	RCALL	TELEGRAM_RESET														; Reset parse step counter
0002b5 dff6                      	RCALL	TELEGRAM_CLRBUFFER													; Clear reception buffer
                                 	
0002b6 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMMANDS
                                 
                                 EXECUTE_COMMAND:
                                 
0002b7 2d03
0002b8 7f07
0002b9 2e30                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
0002ba 9509                      	ICALL																		; Call function (address) of Z-pointer
                                 
0002bb 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 BROADCAST_SET:
                                 
0002bc 2d02                      	MOV		TEMP1, MDFLG														; Load & reset current Broadcast Flags in Mode Register
0002bd 7c07                      	ANDI	TEMP1, 0b11000111													; ^
                                 
0002be 9110 0068                 	LDS		TEMP2, RECENT_DAT													; Load & apply recieved Broadcast Flags
0002c0 2b01                      	OR		TEMP1, TEMP2														; ^
                                 
0002c1 2e20                      	MOV		MDFLG, TEMP1														; Save new mode flags
0002c2 9220 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002c4 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 AUTONOMOUS_SET:
                                 
0002c5 2d02
0002c6 6800
0002c7 2e20                      	SFLG	MDFLG, AUTO															; Set AUTO flag in MDFLG
                                 
0002c8 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TESTMODE_SET:
                                 
0002c9 2d02
0002ca 6004
0002cb 2e20                      	SFLG	MDFLG, TEST															; Set TEST flag in MDFLG
                                 
0002cc 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_SET:
                                 
0002cd 2d02
0002ce 6400
0002cf 2e20                      	SFLG	MDFLG, MAP															; Set MAP flag in MDFLG
                                 
0002d0 9220 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002d2 e401                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Start vehicle with mapping seek PWM
0002d3 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
0002d5 940e 02f9                 	CALL	SET_MOTOR_PWM														; ^
                                 
0002d7 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_SET:
                                 
0002d8 940e 019f                 	CALL	TRAJECTORY_COMPILE													; Compile Trajectory
                                 
0002da 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_GET:
                                 
                                 	// If MAPP > 512 bytes, then buffer will overflow in MatLab
                                 	
0002db e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Initialize Y Pointer
0002dc e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
                                 MAPPING_GET_LOOP:
                                 
0002dd 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
0002de 9119                      	LD		TEMP2, Y+															; ^
                                 
0002df 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
0002e0 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
0002e2 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
0002e3 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
0002e5 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
0002e6 f009                      	BREQ	MAPPING_GET_ESC														; ^
                                 
0002e7 cff5                      	RJMP	MAPPING_GET_LOOP													; Loop
                                 
                                 MAPPING_GET_ESC:
                                 
0002e8 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_GET:
                                 
                                 	// If TRAJ > 512 bytes, then buffer will overflow in MatLab
                                 	
0002e9 940e 019f                 	CALL	TRAJECTORY_COMPILER_SETUP											; Compile Trajectory
                                 	
0002eb e0d3                      	LDI		YH, HIGH(TRAJ_TABLE)												; Initialize Y Pointer
0002ec e0c0                      	LDI		YL,  LOW(TRAJ_TABLE)												; ^
                                 
                                 TRAJECTORY_GET_LOOP:
                                 
0002ed 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
0002ee 9119                      	LD		TEMP2, Y+															; ^
                                 
0002ef 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
0002f0 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
0002f2 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
0002f3 940e 027a                 	CALL	SERIAL_WRITE														; ^
                                 
0002f5 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
0002f6 f009                      	BREQ	TRAJECTORY_GET_ESC													; ^
                                 
0002f7 cff5                      	RJMP	TRAJECTORY_GET_LOOP													; Loop
                                 
                                 TRAJECTORY_GET_ESC:
                                 
0002f8 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> DEVICE (RACECAR) CONTROL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOTOR CONTROL
                                 
                                 SET_MOTOR_PWM:
                                 
0002f9 e60a                      	LDI		TEMP1, 0x6A															; Initialize Waveform Generator (Timer2) (0110_1010)
0002fa bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
0002fb 9120 0068                 	LDS		TEMP3, RECENT_DAT													; Load recent recieved telegram data from SRAM
0002fd 9320 006d                 	STS		DUTY_CYCLE, TEMP3													; Store loaded Duty Cycle in SRAM
                                 	
0002ff 2322                      	TST		TEMP3																; Check if recieved Duty Cycle is 0
000300 f029                      	BREQ	SET_MOTOR_BREAK														; Break vehicle if true
                                 
000301 9894                      	CBI		PORTD, PD4															; Disable MOSFET Brake
                                 
000302 940e 0394                 	CALL	DELAY_100uS															; Wait for 100 ?s
                                 
000304 bd23                      	OUT		OCR2, TEMP3															; Set Duty Cycle (PWM) (0-255) on Timer2
                                 
                                 SET_MOTOR_PWM_ESC:
                                 
000305 9508                      	RET																			; Return
                                 
                                 SET_MOTOR_BREAK:
                                 
000306 e000                      	LDI		TEMP1, 0x00															; Disable Timer2 (PWM)
000307 bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
000308 9897                      	CBI 	PORTD, PD7															; Clear PD7 of PORTD
                                 		
000309 940e 0394                 	CALL	DELAY_100uS															; Wait for 100 us
                                 
00030b 9a94                      	SBI		PORTD, PD4															; Enable MOSFET Brake
                                 
00030c cff8                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 SET_MOTOR_MAX:
                                 
00030d 9a97                      	SBI 	PORTD, PD7															; Set PD7 of PORTD
00030e cff6                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOVING AVERAGE
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support an arbitrary SRAM Location!
                                 
                                 MOVAVG:
                                 
00030f 91b0 0071                 	LDS		XH, MOVAVG_RECENT_XH												; Load recent MOVAVG X pointer
000311 91a0 0072                 	LDS		XL, MOVAVG_RECENT_XL												; ^
                                 
000313 9100 0064                 	LDS		TEMP1, ADC_H														; Insert ADC_val from high because of ADLAR
000315 930d                      	ST		X+, TEMP1															; Save ADC_H to SRAM pointer X
                                 
000316 34a0                      	CPI		XL, MOVAVG_SIZE														;
                                 	//CPI		XL, LOW(MOVAVG_TABLE_END)										; Check if Pointer should be reset
000317 f409                      	BRNE	MOVAVG_SKIP_RESET													; ^
                                 	//CPI		XH, HIGH(MOVAVG_TABLE_END)										; Check if Pointer should be reset
                                 	//BRNE	MOVAVG_SKIP_RESET													; ^
000318 d005                      	RCALL	MOVAVG_POINTER_RESET												; ^
                                 
                                 MOVAVG_SKIP_RESET:
                                 	
                                 	//STS		MOVAVG_RECENT_XH, XH												; - skal med
000319 93a0 0072                 	STS		MOVAVG_RECENT_XL, XL												; -
                                 
00031b d00e                      	RCALL	MOVAVG_ADD															; Do Moving Average Addition
00031c d018                      	RCALL	MOVAVG_DIVIDE														; Do Moving Average Division
                                 
00031d 9508                      	RET																			; Return
                                 
                                 MOVAVG_POINTER_RESET:
                                 
00031e e0b1                      	LDI		XH, HIGH(MOVAVG_TABLE)												; Load reset values into X Pointer
00031f e0a0                      	LDI		XL,  LOW(MOVAVG_TABLE)												; ^
                                 
000320 9508                      	RET																			; Return
                                 
                                 MOVAVG_SRAM_SETUP:
                                 
000321 e400                      	LDI		TEMP1, MOVAVG_SIZE													; Load size of Moving Average filter into register
                                 
000322 93b0 0071                 	STS		MOVAVG_RECENT_XH, XH												; Store location of X pointer into SRAM
000324 93a0 0072                 	STS		MOVAVG_RECENT_XL, XL												; ^
                                 
                                 MOVAVG_SRAM_SETUP_LOOP:
                                 	
000326 930d                      	ST		X+, TEMP1															; Set all values of SRAM to default value
                                 
000327 34a0                      	CPI		XL, LOW(MOVAVG_TABLE_END)											; Check if reached end of table.
000328 f7e9                      	BRNE	MOVAVG_SRAM_SETUP_LOOP												; ^
                                 	//CPI	XH, HIGH(MOVAVG_TABLE_END)						16BIT COMPARE!! CPC	; Check if reached end of table.
                                 	//BRNE	MOVAVG_SRAM_SETUP_LOOP												; ^
                                 
000329 9508                      	RET																			; Return
                                 
                                 MOVAVG_ADD:
                                 	
00032a 2700                      	CLR		TEMP1																; Reset Temporary Register
00032b 2711                      	CLR		TEMP2																; ^
00032c 2722                      	CLR		TEMP3																; ^
                                 
00032d dff0                      	RCALL	MOVAVG_POINTER_RESET												; Reset X Pointer
                                 
                                 MOVAVG_ADD_LOOP:
                                 	
00032e 910d                      	LD		TEMP1, X+ 															; Load value from X pointer location.
00032f 0f10                      	ADD		TEMP2, TEMP1														; Add values
                                 
                                 	// Can be changed to SBIC, SREG ..
                                 	
                                 	//SBRC	SREG, 0
000330 f408                      	BRCC	MOVAVG_ADD_SKIP_CARRY												; Branch if carry is not set - skal slettes
000331 9523                      	INC		TEMP3																; ^
                                 
                                 MOVAVG_ADD_SKIP_CARRY:															;skal slettes!
                                 	
000332 34a0                      	CPI		XL, LOW(MOVAVG_TABLE_END)											; Check if reached end of table
000333 f7d1                      	BRNE	MOVAVG_ADD_LOOP														; 
                                 	//CPI		XH, HIGH(MOVAVG_TABLE_END) THIS IS WRONG SHOULD BE CPC 16bit if ever moved!
                                 	//BRNE	MOVAVG_ADD_LOOP
                                 
000334 9508                      	RET																			; Return
                                 
                                 MOVAVG_DIVIDE:
                                 
000335 e006                      	LDI		TEMP1, MOVAVG_DIVS													; Load number of Divisions
                                 
                                 MOVAVG_DIVIDE_LOOP:
                                 
000336 9525                      	ASR		TEMP3																; TEMP3 IS HIGH
000337 9517                      	ROR		TEMP2																; TEMP2 IS LOW
                                 
000338 950a                      	DEC		TEMP1																; Perform 16 bit divison until done
000339 f7e1                      	BRNE	MOVAVG_DIVIDE_LOOP													; ^
                                 
