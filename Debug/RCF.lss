                                 ; >> BROADCAST
                                 
                                 BROADCAST:
                                 	
000247 fe14                      	SBRS	FNFLG, TMR1															; Check if broadcast is synchronized with CLOCK (Timer1)
000248 9508                      	RET																			; ^
                                 	
000249 e300                      	LDI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Mask Broadcast Modes
00024a 2100                      	AND		TEMP1, MDFLG														; ^
                                 
00024b 3100                      	CPI		TEMP1, (1<<BROD1)													; Tachometer Mode		= (011)
00024c f031                      	BREQ	BROADCAST_TACHOMETER												; ^
                                 	
00024d 3300                      	CPI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Finishline Mode		= (111)
00024e f091                      	BREQ	BROADCAST_FINISHLINE												; ^
                                 
00024f 3200                      	CPI		TEMP1, (1<<BROD2)													; Accelerometer Mode	= (101)
000250 f059                      	BREQ	BROADCAST_ACCELEROMETER												; ^
                                 
000251 d014                      	RCALL	BROADCAST_ALL														; Broadcast All			= (001)
                                 
000252 9508                      	RET																			; Return
                                 
                                 BROADCAST_TACHOMETER:
                                 
000253 9150 0060                 	LDS		TXREG, TACHOMETER_H													; Load & transmit HIGH byte of Tachometer data
000255 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
000257 9150 0061                 	LDS		TXREG, TACHOMETER_L													; Load & transmit HIGH byte of Tachometer data
000259 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
00025b 9508                      	RET																			; Return
                                 
                                 BROADCAST_ACCELEROMETER:
                                 
00025c 9150 0073                 	LDS		TXREG, ACCELEROMETER												; Load & transmit Accelerometer data
00025e 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
000260 9508                      	RET																			; Return
                                 
                                 BROADCAST_FINISHLINE:
                                 	
000261 9150 0070                 	LDS		TXREG, FINISHLINE													; Load & transmit Finishline data
000263 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
000265 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL:
                                 
000266 9100 0061                 	LDS		TEMP1, TACHOMETER_L													; Load current & recent Tachometer (LOW) value
000268 9110 0076                 	LDS		TEMP2, TACHOMETER_L_PREV											;
                                 
00026a 1701                      	CP		TEMP1, TEMP2														; Compare and only transmit if value has changed
00026b f409                      	BRNE	BROADCAST_ALL_SEND													; ^
                                 
00026c 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL_SEND:
                                 
00026d 9300 0076                 	STS		TACHOMETER_L_PREV, TEMP1											; Update recent Tachometer (LOW) value
                                 
00026f dfe3                      	RCALL	BROADCAST_TACHOMETER												; Broadcast Tachometer
000270 dfeb                      	RCALL	BROADCAST_ACCELEROMETER												; Broadcast Accelerometer
                                 
000271 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> COMMUNICATION PROTOCOL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > USART
                                 
                                 SERIAL_READ:
                                 	
000272 9b5f                      	SBIS	UCSRA, RXC															; Wait for Recieve (RXC) Flag
000273 cffe                      	RJMP	SERIAL_READ															; ^
                                 
000274 b14c                      	IN		RXREG, UDR															; Read data into Reception Register
000275 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
000277 9508                      	RET																			; Return
                                 
                                 SERIAL_WRITE:
                                 	
000278 9b5d                      	SBIS	UCSRA, UDRE															; Wait for Empty Transmit Buffer (UDRE) Flag
000279 cffe                      	RJMP	SERIAL_WRITE														; ^
                                 
00027a b95c                      	OUT		UDR, TXREG															; Write data from Transmission Register
                                 
00027b 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > TELEGRAM PARSER
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support recent Z pointer from SRAM!
                                 
                                 TELEGRAM_CHECK:
                                 
00027c 9b5f                      	SBIS	UCSRA, RXC															; Return if no data in USART reception buffer
00027d 9508                      	RET																			; ^
                                 
00027e b14c                      	IN		RXREG, UDR															; Read data into Reception Register
00027f 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
                                 TELEGRAM_PARSER:
                                 	
000281 9100 0066                 	LDS		TEMP1, TEL_STEP														; Load, increment & store Telegram Step Counter
000283 9503                      	INC		TEMP1																; ^
000284 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
000286 3001                      	CPI		TEMP1, 1															; Setup Telegram Parser if Step = 1
000287 f041                      	BREQ	TELEGRAM_PARSE_SETUP												; ^
                                 
000288 3004                      	CPI		TEMP1, 4															; Execute Telegram if Step = 4
000289 f089                      	BREQ	TELEGRAM_EXECUTE													; ^
                                 
00028a 3002                      	CPI		TEMP1, 2															; Parse (TYPE) if Step = 2
00028b f041                      	BREQ	TELEGRAM_JUMP														; ^
                                 
00028c 95ea                      	DEC		ZL																	; Offset Z pointer (-1) to parse TYPE (0x00_XX)
                                 
00028d 3003                      	CPI		TEMP1, 3															; Parse (COMMAND) if Step = 3
00028e f029                      	BREQ	TELEGRAM_JUMP														; ^
                                 
                                 TELEGRAM_PARSER_ESC:
                                 	
00028f 9508                      	RET
                                 
                                 TELEGRAM_PARSE_SETUP:
                                 
000290 e0f0                      	LDI		ZH, HIGH(COMMANDS*2)												; Reset Z Pointer to COMMANDS jump table
000291 e5e0                      	LDI 	ZL,  LOW(COMMANDS*2)												; ^
                                 
000292 95e3                      	INC		ZL																	; Offset Z pointer (+1) to parse COMMAND (0xXX_00)
                                 
000293 cfed                      	RJMP	TELEGRAM_PARSER														; Return
                                 
                                 TELEGRAM_JUMP:
                                 
000294 9634                      	ADIW	ZH:ZL, 4															; Increment Z Pointer (by 4)
                                 
000295 9104                      	LPM		TEMP1, Z															; Load the matching duty cycle
                                 
000296 3e0e                      	CPI		TEMP1, 0xEE															; Reset everything if out of table bounds
000297 f0b1                      	BREQ	TELEGRAM_ERROR														; ^
                                 
000298 1704                      	CP		TEMP1, RXREG														; Find match in jump table
000299 f3a9                      	BREQ	TELEGRAM_PARSER_ESC													; ^
                                 
00029a cff9                      	RJMP	TELEGRAM_JUMP														; Repeat
                                 
                                 TELEGRAM_EXECUTE:
                                 
00029b 9632                      	ADIW	ZH:ZL, 2															; Point at & read LOW of Table address
00029c 9104                      	LPM		TEMP1, Z															; ^
                                 
00029d 9631                      	ADIW	ZH:ZL, 1															; Point at & read HIGH of Table address
00029e 9114                      	LPM		TEMP2, Z															; ^
                                 
00029f 2fe0                      	MOV		ZL, TEMP1															; Load Z Pointer
0002a0 2ff1                      	MOV		ZH, TEMP2															; ^
                                 
0002a1 9340 0068                 	STS		RECENT_DAT, RXREG													; Store recieved data in SRAM
                                 
0002a3 2d01
0002a4 6008
0002a5 2e10                      	SFLG	FNFLG, CMDPD														; Set CMDPD flag in FNFLG
                                 
                                 TELEGRAM_RESET:
                                 	
0002a6 2700                      	CLR		TEMP1																; Reset & store parse step counter
0002a7 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
0002a9 9508                      	RET																			; Return
                                 
                                 TELEGRAM_CLRBUFFER:
                                 
0002aa b10c                      	IN		TEMP1, UDR															; Empty buffer
0002ab 995f                      	SBIC	UCSRA, RXC															; ^
0002ac cffd                      	RJMP	TELEGRAM_CLRBUFFER													; ^
                                 
0002ad 9508                      	RET																			; Return
                                 
                                 TELEGRAM_ERROR:
                                 
0002ae 2744                      	CLR		RXREG																; Clear reception register
                                 
0002af 2d01
0002b0 7f07
0002b1 2e10                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
0002b2 dff3                      	RCALL	TELEGRAM_RESET														; Reset parse step counter
0002b3 dff6                      	RCALL	TELEGRAM_CLRBUFFER													; Clear reception buffer
                                 	
0002b4 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMMANDS
                                 
                                 EXECUTE_COMMAND:
                                 
0002b5 2d01
0002b6 7f07
0002b7 2e10                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
0002b8 9509                      	ICALL																		; Call function (address) of Z-pointer
                                 
0002b9 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 BROADCAST_SET:
                                 
0002ba 2d00                      	MOV		TEMP1, MDFLG														; Load & reset current Broadcast Flags in Mode Register
0002bb 7c07                      	ANDI	TEMP1, 0b11000111													; ^
                                 
0002bc 9110 0068                 	LDS		TEMP2, RECENT_DAT													; Load & apply recieved Broadcast Flags
0002be 2b01                      	OR		TEMP1, TEMP2														; ^
                                 
0002bf 2e00                      	MOV		MDFLG, TEMP1														; Save new mode flags
0002c0 9200 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002c2 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 AUTONOMOUS_SET:
                                 
0002c3 2d00
0002c4 6800
0002c5 2e00                      	SFLG	MDFLG, AUTO
                                 
0002c6 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_SET:
                                 
0002c7 2d00
0002c8 6400
0002c9 2e00                      	SFLG	MDFLG, MAP															; Set MAP flag in MDFLG
                                 
0002ca 9200 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002cc e401                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Start vehicle with mapping seek PWM
0002cd 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
0002cf 940e 02f3                 	CALL	SET_MOTOR_PWM														; ^
                                 
0002d1 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_SET:
                                 
0002d2 940e 0185                 	CALL	TRAJECTORY_COMPILE													; Compile Trajectory
                                 
0002d4 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_GET:
                                 
                                 	// If MAPP > 512 bytes, then buffer will overflow in MatLab
                                 	
0002d5 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Initialize Y Pointer
0002d6 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
                                 MAPPING_GET_LOOP:
                                 
0002d7 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
0002d8 9119                      	LD		TEMP2, Y+															; ^
                                 
0002d9 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
0002da 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
0002dc 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
0002dd 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
0002df 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
0002e0 f009                      	BREQ	MAPPING_GET_ESC														; ^
                                 
0002e1 cff5                      	RJMP	MAPPING_GET_LOOP													; Loop
                                 
                                 MAPPING_GET_ESC:
                                 
0002e2 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_GET:
                                 
                                 	// If TRAJ > 512 bytes, then buffer will overflow in MatLab
                                 	
0002e3 940e 0185                 	CALL	TRAJECTORY_COMPILER_SETUP											; Compile Trajectory
                                 	
0002e5 e0d3                      	LDI		YH, HIGH(TRAJ_TABLE)												; Initialize Y Pointer
0002e6 e0c0                      	LDI		YL,  LOW(TRAJ_TABLE)												; ^
                                 
                                 TRAJECTORY_GET_LOOP:
                                 
0002e7 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
0002e8 9119                      	LD		TEMP2, Y+															; ^
                                 
0002e9 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
0002ea 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
0002ec 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
0002ed 940e 0278                 	CALL	SERIAL_WRITE														; ^
                                 
0002ef 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
0002f0 f009                      	BREQ	TRAJECTORY_GET_ESC													; ^
                                 
0002f1 cff5                      	RJMP	TRAJECTORY_GET_LOOP													; Loop
                                 
                                 TRAJECTORY_GET_ESC:
                                 
0002f2 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> DEVICE (RACECAR) CONTROL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOTOR CONTROL
                                 
                                 SET_MOTOR_PWM:
                                 
0002f3 e60a                      	LDI		TEMP1, 0x6A															; Initialize Waveform Generator (Timer2) (0110_1010)
0002f4 bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
0002f5 9120 0068                 	LDS		TEMP3, RECENT_DAT													; Load recent recieved telegram data from SRAM
0002f7 9320 006d                 	STS		DUTY_CYCLE, TEMP3													; Store loaded Duty Cycle in SRAM
                                 	
0002f9 2322                      	TST		TEMP3																; Check if recieved Duty Cycle is 0
0002fa f029                      	BREQ	SET_MOTOR_BREAK														; Break vehicle if true
                                 
0002fb 9894                      	CBI		PORTD, PD4															; Disable MOSFET Brake
                                 
0002fc 940e 0371                 	CALL	DELAY_100uS															; Wait for 100 ?s
                                 
0002fe bd23                      	OUT		OCR2, TEMP3															; Set Duty Cycle (PWM) (0-255) on Timer2
                                 
                                 SET_MOTOR_PWM_ESC:
                                 
0002ff 9508                      	RET																			; Return
                                 
                                 SET_MOTOR_BREAK:
                                 
000300 e000                      	LDI		TEMP1, 0x00															; Disable Timer2 (PWM)
000301 bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
000302 9897                      	CBI 	PORTD, PD7															; Clear PD7 of PORTD
                                 		
000303 940e 0371                 	CALL	DELAY_100uS															; Wait for 100 us
                                 
000305 9a94                      	SBI		PORTD, PD4															; Enable MOSFET Brake
                                 
000306 cff8                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 SET_MOTOR_MAX:
                                 
000307 9a97                      	SBI 	PORTD, PD7															; Set PD7 of PORTD
000308 cff6                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOVING AVERAGE
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support an arbitrary SRAM Location!
                                 
                                 MOVAVG:
                                 
000309 91b0 0071                 	LDS		XH, MOVAVG_RECENT_XH												; Load recent MOVAVG X pointer
00030b 91a0 0072                 	LDS		XL, MOVAVG_RECENT_XL												; ^
                                 
00030d 9100 0064                 	LDS		TEMP1, ADC_H														; Insert ADC_val from high because of ADLAR
00030f 930d                      	ST		X+, TEMP1															; Save ADC_H to SRAM pointer X
                                 
000310 34a0                      	CPI		XL, MOVAVG_SIZE														;
                                 	//CPI		XL, LOW(MOVAVG_TABLE_END)										; Check if Pointer should be reset
000311 f409                      	BRNE	MOVAVG_SKIP_RESET													; ^
                                 	//CPI		XH, HIGH(MOVAVG_TABLE_END)										; Check if Pointer should be reset
                                 	//BRNE	MOVAVG_SKIP_RESET													; ^
000312 d005                      	RCALL	MOVAVG_POINTER_RESET												; ^
                                 
                                 MOVAVG_SKIP_RESET:
                                 	
                                 	//STS		MOVAVG_RECENT_XH, XH												; - skal med
000313 93a0 0072                 	STS		MOVAVG_RECENT_XL, XL												; -
                                 
000315 d00e                      	RCALL	MOVAVG_ADD															; Do Moving Average Addition
000316 d018                      	RCALL	MOVAVG_DIVIDE														; Do Moving Average Division
                                 
000317 9508                      	RET																			; Return
                                 
                                 MOVAVG_POINTER_RESET:
                                 
000318 e0b1                      	LDI		XH, HIGH(MOVAVG_TABLE)												; Load reset values into X Pointer
000319 e0a0                      	LDI		XL,  LOW(MOVAVG_TABLE)												; ^
                                 
00031a 9508                      	RET																			; Return
                                 
                                 MOVAVG_SRAM_SETUP:
                                 
00031b e400                      	LDI		TEMP1, MOVAVG_SIZE													; Load size of Moving Average filter into register
                                 
00031c 93b0 0071                 	STS		MOVAVG_RECENT_XH, XH												; Store location of X pointer into SRAM
00031e 93a0 0072                 	STS		MOVAVG_RECENT_XL, XL												; ^
                                 
                                 MOVAVG_SRAM_SETUP_LOOP:
                                 	
000320 930d                      	ST		X+, TEMP1															; Set all values of SRAM to default value
                                 
000321 34a0                      	CPI		XL, LOW(MOVAVG_TABLE_END)											; Check if reached end of table.
000322 f7e9                      	BRNE	MOVAVG_SRAM_SETUP_LOOP												; ^
                                 	//CPI	XH, HIGH(MOVAVG_TABLE_END)						16BIT COMPARE!! CPC	; Check if reached end of table.
                                 	//BRNE	MOVAVG_SRAM_SETUP_LOOP												; ^
                                 
000323 9508                      	RET																			; Return
                                 
                                 MOVAVG_ADD:
                                 	
000324 2700                      	CLR		TEMP1																; Reset Temporary Register
000325 2711                      	CLR		TEMP2																; ^
000326 2722                      	CLR		TEMP3																; ^
                                 
000327 dff0                      	RCALL	MOVAVG_POINTER_RESET												; Reset X Pointer
                                 
                                 MOVAVG_ADD_LOOP:
                                 	
000328 910d                      	LD		TEMP1, X+ 															; Load value from X pointer location.
000329 0f10                      	ADD		TEMP2, TEMP1														; Add values
                                 
                                 	// Can be changed to SBIC, SREG ..
                                 	
                                 	//SBRC	SREG, 0
00032a f408                      	BRCC	MOVAVG_ADD_SKIP_CARRY												; Branch if carry is not set - skal slettes
00032b 9523                      	INC		TEMP3																; ^
                                 
                                 MOVAVG_ADD_SKIP_CARRY:															;skal slettes!
                                 	
00032c 34a0                      	CPI		XL, LOW(MOVAVG_TABLE_END)											; Check if reached end of table
00032d f7d1                      	BRNE	MOVAVG_ADD_LOOP														; 
                                 	//CPI		XH, HIGH(MOVAVG_TABLE_END) THIS IS WRONG SHOULD BE CPC 16bit if ever moved!
                                 	//BRNE	MOVAVG_ADD_LOOP
                                 
00032e 9508                      	RET																			; Return
                                 
                                 MOVAVG_DIVIDE:
                                 
00032f e006                      	LDI		TEMP1, MOVAVG_DIVS													; Load number of Divisions
                                 
                                 MOVAVG_DIVIDE_LOOP:
                                 
000330 9525                      	ASR		TEMP3																; TEMP3 IS HIGH
000331 9517                      	ROR		TEMP2																; TEMP2 IS LOW
                                 
000332 950a                      	DEC		TEMP1																; Perform 16 bit divison until done
000333 f7e1                      	BRNE	MOVAVG_DIVIDE_LOOP													; ^
                                 
000334 9310 0073                 	STS		ACCELEROMETER, TEMP2												; Save value of division into SRAM
                                 
000336 9508                      	RET
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> CONTROL UNIT
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > FLAGS
                                 
                                 LOAD_FLAGS:
                                 	
000337 94f8                      	CLI																			; Disable Interrupts
                                 
000338 9100 006e                 	LDS		TEMP1, FUNC_FLG														; Merge SRAM Function Flags from SRAM with Register Function Flags
00033a 2a10                      	OR		FNFLG, TEMP1														; ^
                                 
00033b 2700                      	CLR		TEMP1																; Reset SRAM Function Flags
00033c 9300 006e                 	STS		FUNC_FLG, TEMP1														; ^
                                 
00033e 9478                      	SEI																			; Enable Interrupts
                                 
00033f 9508                      	RET																			; Return
                                 
