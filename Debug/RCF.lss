                                 LOOP3:
000336 e614                      	LDI		TEMP2, 100
                                 LOOP2:
000337 e624                      	LDI		TEMP3, 100
                                 LOOP1:
000338 952a                          DEC		TEMP3
000339 f7f1                          BRNE	LOOP1
00033a 951a                          DEC		TEMP2
00033b f7d9                          BRNE	LOOP2
