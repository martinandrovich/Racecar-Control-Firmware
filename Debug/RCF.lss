00023e e625                      	LDI		TEMP3, TRAJECTORY_TURN_PWM											; Set Motor to Turn PWM
00023f 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000241 940e 02f3                 	CALL	SET_MOTOR_PWM														; ^
                                 
000243 9612                      	ADIW	XH:XL, 2															; Increment Pointers
000244 9622                      	ADIW	YH:YL, 2															; ^
                                 
000245 9508                      	RET																			; Return
                                 
