                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> MAPPING
                                 
                                 MAPPING:
                                 	
000127 fc36                      	SBRC	FNFLG, FNLNE														; Check FNLNE flag
000128 c003                      	RJMP	MAPPING_ISMAP_CHECK													; If 1 then check ISMAP
                                 
000129 fc47                      	SBRC	MTFLG, ISMAP														; Check ISMAP flag
00012a c008                      	RJMP	MAPPING_DEBOUNCE_CHECK												; If 1 then continue Mapping
                                 
00012b c076                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_ISMAP_CHECK:
                                 
00012c 2d03
00012d 7b0f
00012e 2e30                      	CFLG	FNFLG, FNLNE														; Clear FNFNE flag in FNFLG
                                 	
00012f fc47                      	SBRC	MTFLG, ISMAP														; Check ISMAP flag
000130 c02a                      	RJMP	MAPPING_END															; If 1 then end Mapping
                                 
000131 fe47                      	SBRS	MTFLG, ISMAP														; Check ISMAP flag
000132 c016                      	RJMP	MAPPING_BEGIN														; If 0 then begin Mapping
                                 
                                 MAPPING_DEBOUNCE_CHECK:
                                 	
000133 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer value
000135 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 
000137 9110 0074                 	LDS		TEMP2, MAPPING_DEBOUNCE_H											; Load Debounce Tachometer value
000139 9120 0075                 	LDS		TEMP3, MAPPING_DEBOUNCE_L											; ^
                                 
00013b 1782                      	CP		TEMPWL, TEMP3														; Check if Debounce is reached
00013c 0791                      	CPC		TEMPWH, TEMP2 														; ^
00013d f5d0                      	BRSH	MAPPING_CHECK_TURN													; ^
                                 	
00013e 9508                      	RET																			; Return
                                 
                                 MAPPING_DEBOUNCE_RESET:
                                 
00013f 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer value
000141 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 	
000143 960a                      	ADIW	TEMPWH:TEMPWL, MAPPING_DEBOUNCE_VAL									; Update Minumum Tachometer Detection value (current Tachometer + Constant)
                                 
000144 9390 0074                 	STS		MAPPING_DEBOUNCE_H, TEMPWH											; Store values to SRAM
000146 9380 0075                 	STS		MAPPING_DEBOUNCE_L, TEMPWL											; ^
                                 
000148 9508                      	RET																			; Return
                                 	
                                 MAPPING_BEGIN:
                                 
000149 2d04
00014a 7e0f
00014b 2e40                      	CFLG	MTFLG, MPRDY														; Clear MPRDY flag in MTFLG
00014c 2d04
00014d 6800
00014e 2e40                      	SFLG	MTFLG, ISMAP														; Set ISMAP flag in MTFLG
                                 
00014f dfef                      	RCALL	MAPPING_DEBOUNCE_RESET												; Reset Tachometer Debounce											
                                 
000150 e50a                      	LDI		TEMP1, MAPPING_PWM													; Start vehicle with mapping PWM
000151 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
000153 940e 0327                 	CALL	SET_MOTOR_PWM														; ^
                                 
000155 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Initialize Y Pointer
000156 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
000157 2700                      	CLR		TEMP1																; Store 0x0000 into mapping in SRAM
000158 9309                      	ST		Y+, TEMP1															; ^
000159 9309                      	ST		Y+, TEMP1															; ^
                                 
00015a c047                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_END:
                                 
00015b 9110 0060                 	LDS		TEMP2, TACHOMETER_H													; Load current Tachometer values
00015d 9120 0061                 	LDS		TEMP3, TACHOMETER_L													; ^
                                 
00015f 9310 0078                 	STS		TRACK_LENGTH_H, TEMP2 												; Store Track Length (Finishline Tachometer value) into SRAM
000161 9320 0079                 	STS		TRACK_LENGTH_L, TEMP3 												; ^
                                 
                                 	;ST		Y+, TEMP2															; Save finishline tachometer data
                                 	;ST		Y+, TEMP3															;
                                 
000163 ef0f                      	SER		TEMP1																; Store 0xFFFF into mapping in SRAM
000164 9309                      	ST		Y+, TEMP1															; ^
000165 8308                      	ST		Y, TEMP1															; ^
                                 
000166 fe27                      	SBRS	MDFLG, AUTO															; Skip if in Autonomous Mode
000167 940e 0334                 	CALL	SET_MOTOR_BREAK														; Break vehicle
                                 
000169 2d02
00016a 7b0f
00016b 2e20                      	CFLG	MDFLG, MAP															; Clear MAP flag in MDFLG
00016c 2d04
00016d 770f
00016e 2e40                      	CFLG	MTFLG, ISMAP														; Clear ISMAP flag in MTFLG
00016f 2d04
000170 6100
000171 2e40                      	SFLG	MTFLG, MPRDY														; Set MPRDY flag in MTFLG
                                 
000172 2700                      	CLR		TEMP1																; Reset Tachometer
000173 9300 0060                 	STS		TACHOMETER_H, TEMP1													; ^
000175 9300 0061                 	STS		TACHOMETER_L, TEMP1													; ^
                                 
000177 c02a                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN:
                                 
000178 9110 0073                 	LDS		TEMP2, ACCELEROMETER												; Load Accelerometer value
                                 
00017a fc46                      	SBRC	MTFLG, INTURN														; Check INTURN flag
00017b c00c                      	RJMP	MAPPING_CHECK_TURN_OUT												; If 1 then check if a turn has been exited
00017c c000                      	RJMP	MAPPING_CHECK_TURN_IN												; If 0 then check if a turn has been detected
                                 
                                 MAPPING_CHECK_TURN_IN:
                                 
                                 	; Check Right Turn
                                 
00017d 2d04
00017e 6200
00017f 2e40                      	SFLG	MTFLG, TURNDIR														; Set TURNDIR flag in MTFLG
                                 
000180 3713                      	CPI		TEMP2, TURN_TH_IN_RIGHT												; Check Right Turn In
000181 f078                      	BRLO	MAPPING_ADD															; Create Mapping entry if true
                                 
                                 	; Check Left Turn
                                 
000182 2d04
000183 7d0f
000184 2e40                      	CFLG	MTFLG, TURNDIR														; Clear TURNDIR flag in MTFLG
                                 
000185 371a                      	CPI		TEMP2, TURN_TH_IN_LEFT												; Check Left Turn In
000186 f450                      	BRSH	MAPPING_ADD															; Create mapping entry if true
                                 
000187 c01a                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN_OUT:
                                 
000188 fc45                      	SBRC	MTFLG, TURNDIR														; Check TURNDIR flag:
000189 c001                      	RJMP	MAPPING_CHECK_TURN_OUT_RIGHT										; If 1 then check RIGHT turn out
00018a c003                      	RJMP	MAPPING_CHECK_TURN_OUT_LEFT											; If 0 then check LEFT turn out
                                 
                                 MAPPING_CHECK_TURN_OUT_RIGHT:
                                 
00018b 3713                      	CPI		TEMP2, TURN_TH_OUT_RIGHT											; Check Right Turn Out
00018c f420                      	BRSH	MAPPING_ADD															; Create Mapping entry if true
                                 
00018d c014                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_CHECK_TURN_OUT_LEFT:
                                 
00018e 371a                      	CPI		TEMP2, TURN_TH_OUT_LEFT												; Check Left Turn Out
00018f f008                      	BRLO	MAPPING_ADD															; Create mapping entry if true
                                 
000190 c011                      	RJMP	MAPPING_ESC															; Return
                                 
                                 MAPPING_ADD:
                                 
000191 2d04                      	MOV		TEMP1, MTFLG														; Toggle INTURN flag
000192 e410                      	LDI		TEMP2, (1<<INTURN)													; ^
000193 2701                      	EOR		TEMP1, TEMP2														; ^
000194 2e40                      	MOV		MTFLG, TEMP1														; ^
                                 
000195 dfa9                      	RCALL	MAPPING_DEBOUNCE_RESET												; Reset Tachometer Debounce
                                 
000196 9190 0060                 	LDS		TEMPWH, TACHOMETER_H												; Load current Tachometer values
000198 9180 0061                 	LDS		TEMPWL, TACHOMETER_L												; ^
                                 
00019a fc46                      	SBRC	MTFLG, INTURN														; Calculate Offset (In/Out)
00019b 9708                      	SBIW	TEMPWH:TEMPWL, MAPPING_OFFSET_IN									; ^
00019c fe46                      	SBRS	MTFLG, INTURN														; ^
00019d 970b                      	SBIW	TEMPWH:TEMPWL, MAPPING_OFFSET_OUT									; ^
                                 
00019e fd06                      	SBRC	TEMP1, INTURN														; Set MSB of Tachometer (HIGH) to value of INTURN bit
00019f 6890                      	ORI		TEMPWH, (1<<7)														; ^
                                 	
0001a0 9399                      	ST		Y+, TEMPWH															; Store mapping into SRAM
0001a1 9389                      	ST		Y+, TEMPWL															; ^
                                 
                                 MAPPING_ESC:
                                 
0001a2 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> TRAJECTORY
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMPILER
                                 
                                 TRAJECTORY_COMPILE:
                                 TRAJECTORY_COMPILER_SETUP:
                                 
0001a3 e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
0001a4 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 
0001a5 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
0001a6 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
0001a7 9109                      	LD		TEMP1, Y+															; Load first Mapping value
0001a8 9119                      	LD		TEMP2, Y+															; ^
                                 
0001a9 3000                      	CPI		TEMP1, 0															; Perfrom RUNUP if first Mapping value is 0
0001aa f009                      	BREQ	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
0001ab 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_RUNUP:
                                 
0001ac 9109                      	LD		TEMP1, Y+															; Load Mapping value
0001ad 9119                      	LD		TEMP2, Y+															; ^
                                 
0001ae 3f0f                      	CPI		TEMP1, 0xFF															; Loop until EoT is found
0001af f7e1                      	BRNE	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
0001b0 9724                      	SBIW	YH:YL, 4															; Offset the mapping to last outgoing turn in Mapping
                                 
0001b1 9109                      	LD		TEMP1, Y+															; Load Mapping values
0001b2 8118                      	LD		TEMP2, Y															; ^
                                 
0001b3 930d                      	ST		X+, TEMP1															; Store values into Trajectory SRAM
0001b4 931d                      	ST		X+, TEMP2															; ^
                                 
0001b5 9300 007d                 	STS		LAST_TURN_H, TEMP1													; Store Last Turn into SRAM
0001b7 9310 007e                 	STS		LAST_TURN_L, TEMP2													; ^
                                 
0001b9 9190 0078                 	LDS		TEMPWH, TRACK_LENGTH_H 												; Load Track Length
0001bb 9180 0079                 	LDS		TEMPWL, TRACK_LENGTH_L												; ^
                                 
0001bd 1b18                      	SUB		TEMP2, TEMPWL														; Subtract Track Length from Last Turn value (negative expected)
0001be 0b09                      	SBC		TEMP1, TEMPWH														; ^
                                 
0001bf 9310 007c                 	STS		LATEST_STRAIGHT_L, TEMP2											; Store Latest Straight into SRAM
0001c1 9300 007b                 	STS		LATEST_STRAIGHT_H, TEMP1											; ^
                                 
0001c3 e0d2                      	LDI		YH, HIGH(MAPP_TABLE+2)												; Load Y Pointer to (offset) Mapping Table
0001c4 e0c2                      	LDI		YL,  LOW(MAPP_TABLE+2)												; ^
                                 
                                 TRAJECTORY_COMPILER_LOOP:
                                 
0001c5 9119                      	LD		TEMP2, Y+															; Load Mapping values
0001c6 9109                      	LD		TEMP1, Y+															; ^
                                 
0001c7 3f1f                      	CPI		TEMP2, 0xFF															; Check EoT
0001c8 f1b1                      	BREQ	TRAJECTORY_COMPILER_END												; ^
                                 
0001c9 fd17                      	SBRC	TEMP2, MSB															; Check MSB:
0001ca c00a                      	RJMP	TRAJECTORY_COMPILER_BREAK											; if 1 then Break
0001cb c000                      	RJMP	TRAJECTORY_COMPILER_ACCELERATE										; if 0 then Accelerate
                                 	
                                 
                                 TRAJECTORY_COMPILER_ACCELERATE:
                                 	
0001cc 01c8                      	MOVW	TEMPWH:TEMPWL, TEMP2:TEMP1											; 
0001cd 9700                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_ACCLR_OFFSET								; Subtract Acceleration Offset
                                 
0001ce 939d                      	ST		X+, TEMPWH															; Store into Trajectory SRAM
0001cf 938d                      	ST		X+, TEMPWL															; ^
                                 
0001d0 9310 007b                 	STS		LATEST_STRAIGHT_H, TEMP2											; Store Latest Straight into SRAM
0001d2 9300 007c                 	STS		LATEST_STRAIGHT_L, TEMP1											; ^
                                 
0001d4 cff0                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK:
                                 	
0001d5 9190 007b                 	LDS		TEMPWH, LATEST_STRAIGHT_H											; Load Latest Straight
0001d7 9180 007c                 	LDS		TEMPWL,	LATEST_STRAIGHT_L											; ^
                                 
0001d9 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 	
0001da 1b08                      	SUB		TEMP1, TEMPWL														; Calculate Latest Straight
0001db 0b19                      	SBC		TEMP2, TEMPWH														; ^
                                 
0001dc 9300 007a                 	STS		LATEST_STRAIGHT, TEMP1												; Store Latest Straight value into SRAM
                                 
0001de d012                      	RCALL	TRAJECTORY_COMPILER_BREAK_OFFSET									; Find Brake Offset
                                 
0001df 9722                      	SBIW	YH:YL, 2															; Reload Mapping values
0001e0 9119                      	LD		TEMP2, Y+															; ^
0001e1 9109                      	LD		TEMP1, Y+															; ^
                                 
0001e2 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 
0001e3 1b02                      	SUB		TEMP1, TEMP3														; Subtract Brake Offset
0001e4 4010                      	SBCI	TEMP2, 0															; ^
                                 
0001e5 f022                      	BRMI	TRAJECTORY_COMPILER_BREAK_NEGATIVE									; Negative Remainder Exception
                                 
                                 TRAJECTORY_COMPILER_BREAK_STORE:
                                 
0001e6 6810                      	SBR		TEMP2, (1<<MSB)														; Set MSB
                                 
0001e7 931d                      	ST		X+, TEMP2															; Store into Trajectory SRAM
0001e8 930d                      	ST		X+,	TEMP1															; ^
                                 
0001e9 cfdb                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK_NEGATIVE:			
                                 
0001ea 9180 0079                 	LDS		TEMPWL, TRACK_LENGTH_L												; Load Track Length
0001ec 9190 0078                 	LDS		TEMPWH, TRACK_LENGTH_H												; ^
                                 
0001ee 0f08                      	ADD		TEMP1, TEMPWL														; Add Negative Remainder with Track Length
0001ef 1f19                      	ADC		TEMP2, TEMPWH														; ^
                                 														
0001f0 cff5                      	RJMP	TRAJECTORY_COMPILER_BREAK_STORE										; Jump back
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET:
                                 	
0001f1 e0f0                      	LDI		ZH, HIGH(BREAK_OFFSET_TABLE*2)										; Load Z Pointer to Break Offset Table
0001f2 e8ec                      	LDI		ZL,  LOW(BREAK_OFFSET_TABLE*2)										; ^
                                 
0001f3 9100 007a                 	LDS		TEMP1, LATEST_STRAIGHT												; Load value of Latest Straight Distance
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP:
                                 
0001f5 9114                      	LPM		TEMP2, Z															; Load value of Z Pointer from Table and increment by 2
0001f6 9632                      	ADIW	ZH:ZL, 2
                                 
0001f7 1710                      	CP		TEMP2, TEMP1														; Find matching Table value
0001f8 f408                      	BRSH	TRAJECTORY_COMPILER_BREAK_OFFSET_END								; ^
                                 
0001f9 cffb                      	RJMP	TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP								; Loop until found
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_END:
                                 
0001fa 9731                      	SBIW	ZH:ZL, 1															; Decrement Z Pointer
0001fb 9124                      	LPM		TEMP3, Z															; Load value of Z Pointer															
                                 
0001fc 940e 02bb                 	CALL	TELEGRAM_RESET														; Reset Telegram due to Z Pointer
                                 
0001fe 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_END:
                                 
0001ff ef0f                      	SER		TEMP1																; Store 0xFFFF into trajectory in SRAM
000200 930d                      	ST		X+, TEMP1															; ^
000201 930c                      	ST		X, TEMP1															; ^
                                 
000202 9508                      	RET																			; Return	
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > RUNNER
                                 
                                 TRAJECTORY_RUN:
                                 
000203 fe43                      	SBRS	MTFLG, ISRUN														; Check if Trajectory Runner has been initalized
000204 d00a                      	RCALL	TRAJECTORY_RUN_SETUP												; and initialize if not.
                                 	
000205 910c                      	LD		TEMP1, X															; Check EoT
000206 3f0f                      	CPI		TEMP1, 0xFF															; ^
000207 f121                      	BREQ	TRAJECTORY_RUN_END													; ^
                                 	
000208 9100 0060                 	LDS		TEMP1, TACHOMETER_H													; Load Tachometer values
00020a 9110 0061                 	LDS		TEMP2, TACHOMETER_L													; ^
                                 	
00020c fc42                      	SBRC	MTFLG, ISBRK														; Check ISBRK flag:
00020d c009                      	RJMP	TRAJECTORY_RUN_CHECK_ISBRK											; if 1 then check breaking status
00020e c011                      	RJMP	TRAJECTORY_RUN_CHECK_MSB											; if 0 then check next MSB
                                 
                                 TRAJECTORY_RUN_SETUP:
                                 	
00020f e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
000210 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 	
000211 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
000212 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
000213 2d04
000214 6008
000215 2e40                      	SFLG	MTFLG, ISRUN														; Set ISRUN flag in MTFLG
                                 
000216 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_CHECK_ISBRK:
                                 
000217 9199                      	LD		TEMPWH, Y+															; Load Mapping Tachometer values
000218 9189                      	LD		TEMPWL, Y+															; ^
                                 
000219 9722                      	SBIW	YH:YL, 2															; Undo incrementation of Y Pointer
                                 
00021a 779f                      	CBR		TEMPWH, (1<<MSB)													; Clear MSB for Calculations
                                 	
00021b 970a                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_BRAKE_TOLERANCE							; Subtract Tolerance value
                                 
00021c 1718                      	CP		TEMP2, TEMPWL														; Check if Current Tachometer >= Mapping Tachometer w/ Tolerance
00021d 0709                      	CPC		TEMP1, TEMPWH														; ^
00021e f550                      	BRSH	TRAJECTORY_RUN_TURN													; ^	
                                 
00021f 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_CHECK_MSB:
                                 
000220 919d                      	LD		TEMPWH, X+															; Load Trajectory Tachometer values
000221 918d                      	LD		TEMPWL, X+															; ^
                                 
000222 9712                      	SBIW	XH:XL, 2															; Undo incrementation of X Pointer
                                 
000223 e820                      	LDI		TEMP3, (1<<MSB)														; Extract & Remove MSB of Trajectory Tachometer value
000224 2329                      	AND		TEMP3, TEMPWH														; ^
000225 779f                      	CBR		TEMPWH, (1<<MSB)													; ^
                                 
000226 1718                      	CP		TEMP2, TEMPWL														; Escape if Current Tachometer < Trajectory Tachometer
000227 0709                      	CPC		TEMP1, TEMPWH														; ^ 
000228 f158                      	BRLO	TRAJECTORY_RUN_ESC													; ^
                                 
000229 fd27                      	SBRC	TEMP3, MSB															; Check MSB:
00022a c009                      	RJMP	TRAJECTORY_RUN_BREAK												; if 1 then Break
00022b c015                      	RJMP	TRAJECTORY_RUN_ACCELERATE											; if 0 then Accelerate
                                 
                                 TRAJECTORY_RUN_END:
                                 
00022c 2d04
00022d 7f07
00022e 2e40                      	CFLG	MTFLG, ISRUN														; Clear ISRUN in MTFLG
                                 	
00022f 940e 035d                 	CALL	MOVAVG_POINTER_RESET												; Reset Moving Average Pointer
000231 940e 0360                 	CALL	MOVAVG_SRAM_SETUP													; Initialize allocated Moving Average SRAM to default
                                 
000233 9508                      	RET
                                 
                                 TRAJECTORY_RUN_BREAK:
                                 
000234 9190 007d                 	LDS		TEMPWH, LAST_TURN_H													; Load value of Last Turn
000236 9180 007e                 	LDS		TEMPWL, LAST_TURN_L													; ^
                                 
000238 1718                      	CP		TEMP2, TEMPWL														; Escape if Current Tachometer >= Last Turn
000239 0709                      	CPC		TEMP1, TEMPWH														; ^ 
00023a f4c8                      	BRSH	TRAJECTORY_RUN_ESC													; ^
                                 	
00023b 2d04
00023c 6004
00023d 2e40                      	SFLG	MTFLG, ISBRK														; Set ISBRK in MTFLG
                                 	
00023e 940e 0334                 	CALL	SET_MOTOR_BREAK														; Set Motor to Brake PWM
                                 
000240 9508                      	RET																			; Return		
                                 
                                 TRAJECTORY_RUN_ACCELERATE:
                                 	
000241 ef2f                      	LDI		TEMP3, TRAJECTORY_ACCLR_PWM											; Set Motor to Acceleration PWM
000242 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000244 940e 0327                 	CALL	SET_MOTOR_PWM														; ^
                                 
000246 9612                      	ADIW	XH:XL, 2															; Increment Pointers
000247 9622                      	ADIW	YH:YL, 2															; ^
                                 
000248 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_TURN:
                                 	
000249 2d04
00024a 7f0b
00024b 2e40                      	CFLG	MTFLG, ISBRK														; Clear ISBRK flag in MTFLG
                                 
00024c e62e                      	LDI		TEMP3, TRAJECTORY_TURN_PWM											; Set Motor to Turn PWM
00024d 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
00024f 940e 0327                 	CALL	SET_MOTOR_PWM														; ^
                                 
000251 9612                      	ADIW	XH:XL, 2															; Increment Pointers
000252 9622                      	ADIW	YH:YL, 2															; ^
                                 
000253 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_ESC:
                                 	
000254 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> BROADCAST
                                 
                                 BROADCAST:
                                 	
000255 fe34                      	SBRS	FNFLG, TMR1															; Check if broadcast is synchronized with CLOCK (Timer1)
000256 9508                      	RET																			; ^
                                 	
000257 e300                      	LDI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Mask Broadcast Modes
000258 2102                      	AND		TEMP1, MDFLG														; ^
                                 
000259 3100                      	CPI		TEMP1, (1<<BROD1)													; Tachometer Mode		= (011)
00025a f031                      	BREQ	BROADCAST_TACHOMETER												; ^
                                 	
00025b 3300                      	CPI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Velocity Mode			= (111)
00025c f091                      	BREQ	BROADCAST_VELOCITY													; ^
                                 
00025d 3200                      	CPI		TEMP1, (1<<BROD2)													; Accelerometer Mode	= (101)
00025e f059                      	BREQ	BROADCAST_ACCELEROMETER												; ^
                                 
00025f d01b                      	RCALL	BROADCAST_ALL														; Broadcast All			= (001)
                                 
000260 9508                      	RET																			; Return
                                 
                                 BROADCAST_TACHOMETER:
                                 
000261 9150 0060                 	LDS		TXREG, TACHOMETER_H													; Load & transmit HIGH byte of Tachometer data
000263 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
000265 9150 0061                 	LDS		TXREG, TACHOMETER_L													; Load & transmit HIGH byte of Tachometer data
000267 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
000269 9508                      	RET																			; Return
                                 
                                 BROADCAST_ACCELEROMETER:
                                 
00026a 9150 0073                 	LDS		TXREG, ACCELEROMETER												; Load & transmit Accelerometer data
00026c 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
00026e 9508                      	RET																			; Return
                                 
                                 BROADCAST_VELOCITY:
                                 
00026f fe32                      	SBRS	FNFLG, VELOC
000270 940e 02ee                 	CALL	VELOCITY_SET
                                 	
000272 9150 0080                 	LDS		TXREG, VELOCITY_H													; Load & transmit Finishline data
000274 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
000276 9150 0081                 	LDS		TXREG, VELOCITY_L													; Load & transmit Finishline data
000278 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
00027a 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL:
                                 
00027b 9100 0061                 	LDS		TEMP1, TACHOMETER_L													; Load current & recent Tachometer (LOW) value
00027d 9110 0076                 	LDS		TEMP2, TACHOMETER_L_PREV											;
                                 
00027f 1701                      	CP		TEMP1, TEMP2														; Compare and only transmit if value has changed
000280 f409                      	BRNE	BROADCAST_ALL_SEND													; ^
                                 
000281 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL_SEND:
                                 
000282 9300 0076                 	STS		TACHOMETER_L_PREV, TEMP1											; Update recent Tachometer (LOW) value
                                 
000284 dfdc                      	RCALL	BROADCAST_TACHOMETER												; Broadcast Tachometer
000285 dfe4                      	RCALL	BROADCAST_ACCELEROMETER												; Broadcast Accelerometer
                                 
000286 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> COMMUNICATION PROTOCOL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > USART
                                 
                                 SERIAL_READ:
                                 	
000287 9b5f                      	SBIS	UCSRA, RXC															; Wait for Recieve (RXC) Flag
000288 cffe                      	RJMP	SERIAL_READ															; ^
                                 
000289 b14c                      	IN		RXREG, UDR															; Read data into Reception Register
00028a 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
00028c 9508                      	RET																			; Return
                                 
                                 SERIAL_WRITE:
                                 	
00028d 9b5d                      	SBIS	UCSRA, UDRE															; Wait for Empty Transmit Buffer (UDRE) Flag
00028e cffe                      	RJMP	SERIAL_WRITE														; ^
                                 
00028f b95c                      	OUT		UDR, TXREG															; Write data from Transmission Register
                                 
000290 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > TELEGRAM PARSER
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support recent Z pointer from SRAM!
                                 
                                 TELEGRAM_CHECK:
                                 
000291 9b5f                      	SBIS	UCSRA, RXC															; Return if no data in USART reception buffer
000292 9508                      	RET																			; ^
                                 
000293 b14c                      	IN		RXREG, UDR															; Read data into Reception Register
000294 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
                                 TELEGRAM_PARSER:
                                 	
000296 9100 0066                 	LDS		TEMP1, TEL_STEP														; Load, increment & store Telegram Step Counter
000298 9503                      	INC		TEMP1																; ^
000299 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
00029b 3001                      	CPI		TEMP1, 1															; Setup Telegram Parser if Step = 1
00029c f041                      	BREQ	TELEGRAM_PARSE_SETUP												; ^
                                 
00029d 3004                      	CPI		TEMP1, 4															; Execute Telegram if Step = 4
00029e f089                      	BREQ	TELEGRAM_EXECUTE													; ^
                                 
00029f 3002                      	CPI		TEMP1, 2															; Parse (TYPE) if Step = 2
0002a0 f041                      	BREQ	TELEGRAM_JUMP														; ^
                                 
0002a1 95ea                      	DEC		ZL																	; Offset Z pointer (-1) to parse TYPE (0x00_XX)
                                 
0002a2 3003                      	CPI		TEMP1, 3															; Parse (COMMAND) if Step = 3
0002a3 f029                      	BREQ	TELEGRAM_JUMP														; ^
                                 
                                 TELEGRAM_PARSER_ESC:
                                 	
0002a4 9508                      	RET
                                 
                                 TELEGRAM_PARSE_SETUP:
                                 
0002a5 e0f0                      	LDI		ZH, HIGH(COMMANDS*2)												; Reset Z Pointer to COMMANDS jump table
0002a6 e5e0                      	LDI 	ZL,  LOW(COMMANDS*2)												; ^
                                 
0002a7 95e3                      	INC		ZL																	; Offset Z pointer (+1) to parse COMMAND (0xXX_00)
                                 
0002a8 cfed                      	RJMP	TELEGRAM_PARSER														; Return
                                 
                                 TELEGRAM_JUMP:
                                 
0002a9 9634                      	ADIW	ZH:ZL, 4															; Increment Z Pointer (by 4)
                                 
0002aa 9104                      	LPM		TEMP1, Z															; Load the matching duty cycle
                                 
0002ab 3e0e                      	CPI		TEMP1, 0xEE															; Reset everything if out of table bounds
0002ac f0b1                      	BREQ	TELEGRAM_ERROR														; ^
                                 
0002ad 1704                      	CP		TEMP1, RXREG														; Find match in jump table
0002ae f3a9                      	BREQ	TELEGRAM_PARSER_ESC													; ^
                                 
0002af cff9                      	RJMP	TELEGRAM_JUMP														; Repeat
                                 
                                 TELEGRAM_EXECUTE:
                                 
0002b0 9632                      	ADIW	ZH:ZL, 2															; Point at & read LOW of Table address
0002b1 9104                      	LPM		TEMP1, Z															; ^
                                 
0002b2 9631                      	ADIW	ZH:ZL, 1															; Point at & read HIGH of Table address
0002b3 9114                      	LPM		TEMP2, Z															; ^
                                 
0002b4 2fe0                      	MOV		ZL, TEMP1															; Load Z Pointer
0002b5 2ff1                      	MOV		ZH, TEMP2															; ^
                                 
0002b6 9340 0068                 	STS		RECENT_DAT, RXREG													; Store recieved data in SRAM
                                 
0002b8 2d03
0002b9 6008
0002ba 2e30                      	SFLG	FNFLG, CMDPD														; Set CMDPD flag in FNFLG
                                 
                                 TELEGRAM_RESET:
                                 	
0002bb 2700                      	CLR		TEMP1																; Reset & store parse step counter
0002bc 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
0002be 9508                      	RET																			; Return
                                 
                                 TELEGRAM_CLRBUFFER:
                                 
0002bf b10c                      	IN		TEMP1, UDR															; Empty buffer
0002c0 995f                      	SBIC	UCSRA, RXC															; ^
0002c1 cffd                      	RJMP	TELEGRAM_CLRBUFFER													; ^
                                 
0002c2 9508                      	RET																			; Return
                                 
                                 TELEGRAM_ERROR:
                                 
0002c3 2744                      	CLR		RXREG																; Clear reception register
                                 
0002c4 2d03
0002c5 7f07
0002c6 2e30                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
0002c7 dff3                      	RCALL	TELEGRAM_RESET														; Reset parse step counter
0002c8 dff6                      	RCALL	TELEGRAM_CLRBUFFER													; Clear reception buffer
                                 	
0002c9 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMMANDS
                                 
                                 EXECUTE_COMMAND:
                                 
0002ca 2d03
0002cb 7f07
0002cc 2e30                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
0002cd 9509                      	ICALL																		; Call function (address) of Z-pointer
                                 
0002ce 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 BROADCAST_SET:
                                 
0002cf 2d02                      	MOV		TEMP1, MDFLG														; Load & reset current Broadcast Flags in Mode Register
0002d0 7c07                      	ANDI	TEMP1, 0b11000111													; ^
                                 
0002d1 9110 0068                 	LDS		TEMP2, RECENT_DAT													; Load & apply recieved Broadcast Flags
0002d3 2b01                      	OR		TEMP1, TEMP2														; ^
                                 
0002d4 2e20                      	MOV		MDFLG, TEMP1														; Save new mode flags
0002d5 9220 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002d7 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 AUTONOMOUS_SET:
                                 
0002d8 2d02
0002d9 6800
0002da 2e20                      	SFLG	MDFLG, AUTO															; Set AUTO flag in MDFLG
                                 
0002db 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 BREAKTEST_SET:
                                 
0002dc 9100 0068                 	LDS		TEMP1, RECENT_DAT													; Set Breaktest Value
0002de 9300 0084                 	STS		BREAK_TEST_VAL, TEMP1												; ^
                                 
0002e0 2700                      	CLR		TEMP1																; Reset Tachometer
0002e1 9300 0060                 	STS		TACHOMETER_H, TEMP1													; ^
0002e3 9300 0061                 	STS		TACHOMETER_L, TEMP1													; ^
                                 	
0002e5 ef2f                      	LDI		TEMP3, TRAJECTORY_ACCLR_PWM											; Set Motor to Acceleration PWM
0002e6 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
0002e8 940e 0327                 	CALL	SET_MOTOR_PWM														; ^
                                 	
0002ea 2d02
0002eb 6004
0002ec 2e20                      	SFLG	MDFLG, TEST															; Set TEST flag in MDFLG
                                 
0002ed 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 VELOCITY_SET:
                                 
0002ee 2d03
0002ef 6004
0002f0 2e30                      	SFLG	FNFLG, VELOC														; Set VELOC flag in FNFLG
                                 
0002f1 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_SET:
                                 
0002f2 2d02
0002f3 6400
0002f4 2e20                      	SFLG	MDFLG, MAP															; Set MAP flag in MDFLG
                                 
0002f5 9220 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002f7 e40e                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Start vehicle with mapping seek PWM
0002f8 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
0002fa 940e 0327                 	CALL	SET_MOTOR_PWM														; ^
                                 
0002fc 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_SET:
                                 
0002fd 940e 01a3                 	CALL	TRAJECTORY_COMPILE													; Compile Trajectory
                                 
0002ff 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_GET:
                                 
                                 	// If MAPP > 512 bytes, then buffer will overflow in MatLab
                                 	
000300 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Initialize Y Pointer
000301 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
                                 MAPPING_GET_LOOP:
                                 
000302 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
000303 9119                      	LD		TEMP2, Y+															; ^
                                 
000304 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
000305 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
000307 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
000308 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
00030a 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
00030b f009                      	BREQ	MAPPING_GET_ESC														; ^
                                 
00030c cff5                      	RJMP	MAPPING_GET_LOOP													; Loop
                                 
                                 MAPPING_GET_ESC:
                                 
00030d 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TACHOMETER_GET:
                                 	
00030e 9150 0060                 	LDS		TXREG, TACHOMETER_H
000310 940e 028d                 	CALL	SERIAL_WRITE
                                 
000312 9150 0061                 	LDS		TXREG, TACHOMETER_L
000314 940e 028d                 	CALL	SERIAL_WRITE
                                 
000316 9508                      	RET
                                 		
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_GET:
                                 
                                 	// If TRAJ > 512 bytes, then buffer will overflow in MatLab
                                 	
000317 940e 01a3                 	CALL	TRAJECTORY_COMPILER_SETUP											; Compile Trajectory
                                 	
000319 e0d3                      	LDI		YH, HIGH(TRAJ_TABLE)												; Initialize Y Pointer
00031a e0c0                      	LDI		YL,  LOW(TRAJ_TABLE)												; ^
                                 
                                 TRAJECTORY_GET_LOOP:
                                 
00031b 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
00031c 9119                      	LD		TEMP2, Y+															; ^
                                 
00031d 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
00031e 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
000320 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
000321 940e 028d                 	CALL	SERIAL_WRITE														; ^
                                 
000323 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
000324 f009                      	BREQ	TRAJECTORY_GET_ESC													; ^
                                 
000325 cff5                      	RJMP	TRAJECTORY_GET_LOOP													; Loop
                                 
                                 TRAJECTORY_GET_ESC:
                                 
000326 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> DEVICE (RACECAR) CONTROL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOTOR CONTROL
                                 
                                 SET_MOTOR_PWM:
                                 
000327 e60a                      	LDI		TEMP1, 0x6A															; Initialize Waveform Generator (Timer2) (0110_1010)
000328 bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
000329 9120 0068                 	LDS		TEMP3, RECENT_DAT													; Load recent recieved telegram data from SRAM
00032b 9320 006d                 	STS		DUTY_CYCLE, TEMP3													; Store loaded Duty Cycle in SRAM
                                 	
00032d 2322                      	TST		TEMP3																; Check if recieved Duty Cycle is 0
00032e f029                      	BREQ	SET_MOTOR_BREAK														; Break vehicle if true
                                 
00032f 9894                      	CBI		PORTD, PD4															; Disable MOSFET Brake
                                 
000330 940e 03cd                 	CALL	DELAY_20uS															; Wait for 100 ?s
                                 
000332 bd23                      	OUT		OCR2, TEMP3															; Set Duty Cycle (PWM) (0-255) on Timer2
                                 
                                 SET_MOTOR_PWM_ESC:
                                 
000333 9508                      	RET																			; Return
                                 
                                 SET_MOTOR_BREAK:
                                 
000334 e000                      	LDI		TEMP1, 0x00															; Disable Timer2 (PWM)
000335 bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
000336 9897                      	CBI 	PORTD, PD7															; Clear PD7 of PORTD
                                 		
000337 940e 03cd                 	CALL	DELAY_20uS															; Wait for 100 us
                                 
000339 9a94                      	SBI		PORTD, PD4															; Enable MOSFET Brake
                                 
00033a cff8                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 SET_MOTOR_MAX:
                                 
00033b 9a97                      	SBI 	PORTD, PD7															; Set PD7 of PORTD
00033c cff6                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > BREAK TEST
                                 
                                 BREAKTEST:
                                 
00033d 9100 0084                 	LDS		TEMP1, BREAK_TEST_VAL												; Load Break Tachometer value
