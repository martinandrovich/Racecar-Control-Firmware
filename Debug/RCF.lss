                                 ;  > FLAGS
                                 
                                 LOAD_FLAGS:
                                 	
00030d 94f8                      	CLI																			; Disable Interrupts
                                 
00030e 9100 006e                 	LDS		TEMP1, FUNC_FLG														; Merge SRAM Function Flags from SRAM with Register Function Flags
000310 2a10                      	OR		FNFLG, TEMP1														; ^
                                 
