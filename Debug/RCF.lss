000167 fc25                      	SBRC	MTFLG, TURNDIR														; Check TURNDIR flag
000168 c001                      	RJMP	MAPPING_CHECK_TURN_OUT_RIGHT										; If SET then check RIGHT turn out
000169 c003                      	RJMP	MAPPING_CHECK_TURN_OUT_LEFT											; If CLR then check LEFT turn out
                                 
