000232 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000234 940e 02f3                 	CALL	SET_MOTOR_PWM														; ^
                                 
000236 9612                      	ADIW	XH:XL, 2															; Increment Pointers
000237 9622                      	ADIW	YH:YL, 2															; ^
                                 
000238 9508                      	RET																			; Return
                                 
