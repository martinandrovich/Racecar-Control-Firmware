000213 0709                      	CPC		TEMP1, TEMPWH														; ^ 
000214 f188                      	BRLO	TRAJECTORY_RUN_ESC													; ^
                                 
000215 fd27                      	SBRC	TEMP3, MSB															; Check MSB:
000216 c009                      	RJMP	TRAJECTORY_RUN_BREAK												; if 1 then Break
000217 c017                      	RJMP	TRAJECTORY_RUN_ACCELERATE											; if 0 then Accelerate
                                 
                                 TRAJECTORY_RUN_END:
                                 
000218 2d02
000219 7f07
00021a 2e20                      	CFLG	MTFLG, ISRUN														; Clear ISRUN in MTFLG
                                 	
00021b 940e 0318                 	CALL	MOVAVG_POINTER_RESET												; Reset Moving Average Pointer
