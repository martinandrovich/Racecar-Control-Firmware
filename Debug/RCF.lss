000204 9189                      	LD		TEMPWL, Y+															; ^
                                 
000205 9722                      	SBIW	YH:YL, 2															; Undo incrementation of Y Pointer
                                 
000206 779f                      	CBR		TEMPWH, (1<<MSB)													; Clear MSB for Calculations
                                 	
000207 9705                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_BRAKE_TOLERANCE							; Subtract Tolerance value
                                 
000208 1718                      	CP		TEMP2, TEMPWL														; Check if Current Tachometer >= Mapping Tachometer w/ Tolerance
000209 0709                      	CPC		TEMP1, TEMPWH														; ^
