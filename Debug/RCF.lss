                                 LOOP2:
000346 e624                      	LDI		TEMP3, 100
                                 LOOP1:
000347 952a                          DEC		TEMP3
000348 f7f1                          BRNE	LOOP1
000349 951a                          DEC		TEMP2
00034a f7d9                          BRNE	LOOP2
00034b 950a                          DEC		TEMP1
