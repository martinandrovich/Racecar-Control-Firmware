000048 0a5b                      	.DB		91, 10
000049 0a6d                      	.DB		109, 10
00004a 0a80                      	.DB		128, 10
00004b 0a92                      	.DB		146, 10
00004c 0aa4                      	.DB		164, 10						
00004d 0ab6                      	.DB		182, 10
00004e 0ac9                      	.DB		201, 10
