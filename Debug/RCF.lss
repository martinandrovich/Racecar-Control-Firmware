000229 2d02
00022a 6004
00022b 2e20                      	SFLG	MTFLG, ISBRK														; Set ISBRK in MTFLG
                                 	
00022c 940e 0300                 	CALL	SET_MOTOR_BREAK														; Set Motor to Brake PWM
                                 
