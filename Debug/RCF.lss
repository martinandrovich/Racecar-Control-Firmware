                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> TRAJECTORY
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMPILER
                                 TRAJECTORY_COMPILE:
                                 TRAJECTORY_COMPILER_SETUP:
                                 
000182 e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
000183 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 
000184 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
000185 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
000186 9109                      	LD		TEMP1, Y+															; Load first Mapping value
000187 9119                      	LD		TEMP2, Y+															; ^
                                 
000188 3000                      	CPI		TEMP1, 0															; Perfrom RUNUP if first Mapping value is 0
000189 f009                      	BREQ	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
00018a 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_RUNUP:
                                 
00018b 9109                      	LD		TEMP1, Y+															; Load Mapping value
00018c 9119                      	LD		TEMP2, Y+															; ^
                                 
00018d 3f0f                      	CPI		TEMP1, 0xFF															; Loop until EoT is found
00018e f7e1                      	BRNE	TRAJECTORY_COMPILER_RUNUP											; ^
                                 
00018f 9724                      	SBIW	Y, 4																; Offset the mapping to last outgoing turn in Mapping
                                 
000190 9109                      	LD		TEMP1, Y+															; Load Mapping values
000191 8118                      	LD		TEMP2, Y															; ^
                                 
000192 930d                      	ST		X+, TEMP1															; Store values into Trajectory SRAM
000193 931d                      	ST		X+, TEMP2															; ^
                                 
000194 9190 0078                 	LDS		TEMPWH, TRACK_LENGTH_H 												; Load Track Length
000196 9180 0079                 	LDS		TEMPWL, TRACK_LENGTH_L												; ^
                                 
000198 1b18                      	SUB		TEMP2, TEMPWL														; Subtract Track Length from Last Turn value (negative expected)
000199 0b09                      	SBC		TEMP1, TEMPWH														; ^
                                 
00019a 9310 007c                 	STS		LATEST_STRAIGHT_L, TEMP2											; Store Latest Straight into SRAM
00019c 9300 007b                 	STS		LATEST_STRAIGHT_H, TEMP1											; ^
                                 
                                 	;STS		LATEST_STRAIGHT, TEMP2
                                 
00019e e0d2                      	LDI		YH, HIGH(MAPP_TABLE+2)												; Load Y Pointer to (offset) Mapping Table
00019f e0c2                      	LDI		YL,  LOW(MAPP_TABLE+2)												; ^
                                 
                                 TRAJECTORY_COMPILER_LOOP:
                                 
0001a0 9119                      	LD		TEMP2, Y+															; Load Mapping values
0001a1 9109                      	LD		TEMP1, Y+															; ^
                                 
0001a2 3f1f                      	CPI		TEMP2, 0xFF															; Check EoT
0001a3 f171                      	BREQ	TRAJECTORY_COMPILER_END												; ^
                                 
0001a4 fd17                      	SBRC	TEMP2, MSB															; Check MSB:
0001a5 c00a                      	RJMP	TRAJECTORY_COMPILER_BREAK											; if 1 then Break
0001a6 c000                      	RJMP	TRAJECTORY_COMPILER_ACCELERATE										; if 0 then Accelerate
                                 	
                                 
                                 TRAJECTORY_COMPILER_ACCELERATE:
                                 	
                                 	// Unddig brug af MOVW?
                                 	
0001a7 01c8                      	MOVW	TEMPWH:TEMPWL, TEMP2:TEMP1											; 
0001a8 9700                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_ACCLR_OFFSET								; Subtract Acceleration Offset
                                 
0001a9 939d                      	ST		X+, TEMPWH															; Store into Trajectory SRAM
0001aa 938d                      	ST		X+, TEMPWL															; ^
                                 
0001ab 9310 007b                 	STS		LATEST_STRAIGHT_H, TEMP2											; Store Latest Straight into SRAM
0001ad 9300 007c                 	STS		LATEST_STRAIGHT_L, TEMP1											; ^
                                 
0001af cff0                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK:
                                 	
0001b0 9190 007b                 	LDS		TEMPWH, LATEST_STRAIGHT_H											; Load Latest Straight
0001b2 9180 007c                 	LDS		TEMPWL,	LATEST_STRAIGHT_L											; ^
                                 
0001b4 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 	
0001b5 1b08                      	SUB		TEMP1, TEMPWL														; Calculate Latest Straight
0001b6 0b19                      	SBC		TEMP2, TEMPWH														; ^
                                 
0001b7 9300 007a                 	STS		LATEST_STRAIGHT, TEMP1												; Store Latest Straight value into SRAM
                                 
0001b9 d00a                      	RCALL	TRAJECTORY_COMPILER_BREAK_OFFSET									; Find Brake Offset
                                 
0001ba 9722                      	SBIW	YH:YL, 2															; Reload Mapping values
0001bb 9119                      	LD		TEMP2, Y+															; ^
0001bc 9109                      	LD		TEMP1, Y+															; ^
                                 
0001bd 771f                      	CBR		TEMP2, (1<<MSB)														; Clear MSB for Calculations
                                 
0001be 1b02                      	SUB		TEMP1, TEMP3														; Subtract Brake Offset
0001bf 4010                      	SBCI	TEMP2, 0															; ^
                                 
0001c0 6810                      	SBR		TEMP2, (1<<MSB)														; Set MSB
                                 
0001c1 931d                      	ST		X+, TEMP2															; Store into Trajectory SRAM
0001c2 930d                      	ST		X+,	TEMP1															; ^
                                 
0001c3 cfdc                      	RJMP	TRAJECTORY_COMPILER_LOOP											; Loop
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET:
                                 	
0001c4 e0f0                      	LDI		ZH, HIGH(BREAK_OFFSET_TABLE*2)										; Load Z Pointer to Break Offset Table
0001c5 e8e8                      	LDI		ZL,  LOW(BREAK_OFFSET_TABLE*2)										; ^
                                 
0001c6 9100 007a                 	LDS		TEMP1, LATEST_STRAIGHT												; Load value of Latest Straight Distance
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP:
                                 
0001c8 9114                      	LPM		TEMP2, Z															; Load value of Z Pointer from Table and increment by 2
0001c9 9632                      	ADIW	ZH:ZL, 2
                                 
0001ca 1710                      	CP		TEMP2, TEMP1														; Find matching Table value
0001cb f408                      	BRSH	TRAJECTORY_COMPILER_BREAK_OFFSET_END								; ^
                                 
0001cc cffb                      	RJMP	TRAJECTORY_COMPILER_BREAK_OFFSET_LOOP
                                 
                                 TRAJECTORY_COMPILER_BREAK_OFFSET_END:
                                 
0001cd 9731                      	SBIW	ZH:ZL, 1															; Decrement Z Pointer
0001ce 9124                      	LPM		TEMP3, Z															; Load value of Z Pointer															
                                 
0001cf 940e 0280                 	CALL	TELEGRAM_RESET														; Reset Telegram due to Z Pointer
                                 
0001d1 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_COMPILER_END:
                                 
0001d2 ef0f                      	SER		TEMP1																; Store 0xFFFF into trajectory in SRAM
0001d3 930d                      	ST		X+, TEMP1															; ^
0001d4 930c                      	ST		X, TEMP1															; ^
                                 
0001d5 9508                      	RET																			; Return	
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > RUNNER
                                 
                                 TRAJECTORY_RUN:
                                 
0001d6 fe23                      	SBRS	MTFLG, ISRUN														; Check if Trajectory Runner has been initalized
0001d7 d00a                      	RCALL	TRAJECTORY_RUN_SETUP												; and initialize if not.
                                 	
0001d8 910c                      	LD		TEMP1, X															; Check EoT
0001d9 3f0f                      	CPI		TEMP1, 0xFF															; ^
0001da f1f1                      	BREQ	TRAJECTORY_RUN_END													; ^
                                 	
0001db 9100 0060                 	LDS		TEMP1, TACHOMETER_H													; Load Tachometer values
0001dd 9110 0061                 	LDS		TEMP2, TACHOMETER_L													; ^
                                 	
0001df fc22                      	SBRC	MTFLG, ISBRK														; Check ISBRK FLAG
0001e0 c009                      	RJMP	TRAJECTORY_RUN_CHECK_ISBRK											;
0001e1 c011                      	RJMP	TRAJECTORY_RUN_CHECK_MSB											;
                                 
                                 TRAJECTORY_RUN_SETUP:
                                 	
0001e2 e0b3                      	LDI		XH, HIGH(TRAJ_TABLE)												; Load X Pointer to Trajectory Table
0001e3 e0a0                      	LDI		XL,  LOW(TRAJ_TABLE)												; ^
                                 	
0001e4 e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Load Y Pointer to Mapping Table
0001e5 e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
0001e6 2d02
0001e7 6008
0001e8 2e20                      	SFLG	MTFLG, ISRUN														; Set ISRUN flag in MTFLG
                                 
0001e9 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_CHECK_ISBRK:
                                 
0001ea 9199                      	LD		TEMPWH, Y+															; Load Mapping Tachometer values
0001eb 9189                      	LD		TEMPWL, Y+															; ^
                                 
0001ec 9722                      	SBIW	YH:YL, 2															; Undo incrementation of Y Pointer
                                 
0001ed 779f                      	CBR		TEMPWH, (1<<MSB)													; Clear MSB for Calculations
                                 	
0001ee 9700                      	SBIW	TEMPWH:TEMPWL, TRAJECTORY_BRAKE_TOLERANCE							; Subtract Tolerance value
                                 
0001ef 1718                      	CP		TEMP2, TEMPWL														; Check if Current Tachometer >= Mapping Tachometer w/ Tolerance
0001f0 0709                      	CPC		TEMP1, TEMPWH														; ^
0001f1 f4d8                      	BRSH	TRAJECTORY_RUN_TURN													; ^	
                                 
0001f2 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_CHECK_MSB:
                                 
0001f3 919d                      	LD		TEMPWH, X+															; Load Trajectory Tachometer values
0001f4 918d                      	LD		TEMPWL, X+															; ^
                                 
0001f5 9712                      	SBIW	XH:XL, 2															; Undo incrementation of X Pointer
                                 
0001f6 e820                      	LDI		TEMP3, (1<<MSB)														; Extract & Remove MSB of Trajectory Tachometer value
0001f7 2329                      	AND		TEMP3, TEMPWH														; ^
0001f8 779f                      	CBR		TEMPWH, (1<<MSB)													; ^
                                 
0001f9 1718                      	CP		TEMP2, TEMPWL														; Escape if Current Tachometer < Trajectory Tachometer
0001fa 0709                      	CPC		TEMP1, TEMPWH														; ^ 
0001fb f0e0                      	BRLO	TRAJECTORY_RUN_ESC													; ^
                                 
0001fc fd27                      	SBRC	TEMP3, MSB															; Check MSB:
0001fd c001                      	RJMP	TRAJECTORY_RUN_BREAK												; if 1 then Break
0001fe c006                      	RJMP	TRAJECTORY_RUN_ACCELERATE											; if 0 then Accelerate
                                 
                                 TRAJECTORY_RUN_BREAK:
                                 
0001ff 2d02
000200 6004
000201 2e20                      	SFLG	MTFLG, ISBRK														; Set ISBRK in MTFLG
                                 	
000202 940e 02d6                 	CALL	SET_MOTOR_BREAK														; Set Motor to Brake PWM
                                 
000204 9508                      	RET																			; Return		
                                 
                                 TRAJECTORY_RUN_ACCELERATE:
                                 
000205 ef2f                      	LDI		TEMP3, TRAJECTORY_ACCLR_PWM											; Set Motor to Acceleration PWM
000206 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000208 940e 02c9                 	CALL	SET_MOTOR_PWM														; ^
                                 
00020a 9612                      	ADIW	XH:XL, 2															; Increment Pointers
00020b 9622                      	ADIW	YH:YL, 2															; ^
                                 
00020c 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_TURN:
                                 
00020d 2d02
00020e 7f0b
00020f 2e20                      	CFLG	MTFLG, ISBRK														; Clear ISBRK flag in MTFLG
                                 
000210 e625                      	LDI		TEMP3, TRAJECTORY_TURN_PWM											; Set Motor to Turn PWM
000211 9320 0068                 	STS		RECENT_DAT, TEMP3													; ^
000213 940e 02c9                 	CALL	SET_MOTOR_PWM														; ^
                                 
000215 9612                      	ADIW	XH:XL, 2															; Increment Pointers
000216 9622                      	ADIW	YH:YL, 2															; ^
                                 
000217 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_ESC:
                                 	
000218 9508                      	RET																			; Return
                                 
                                 TRAJECTORY_RUN_END:
                                 
000219 2d02
00021a 7f07
00021b 2e20                      	CFLG	MTFLG, ISRUN														; Clear ISRUN in MTFLG
                                 	
00021c 940e 02ee                 	CALL	MOVAVG_POINTER_RESET												; Reset Moving Average Pointer
00021e 940e 02f1                 	CALL	MOVAVG_SRAM_SETUP													; Initialize allocated Moving Average SRAM to default
                                 
000220 9508                      	RET
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> BROADCAST
                                 
                                 BROADCAST:
                                 	
000221 fe14                      	SBRS	FNFLG, TMR1															; Check if broadcast is synchronized with CLOCK (Timer1)
000222 9508                      	RET																			; ^
                                 	
000223 e300                      	LDI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Mask Broadcast Modes
000224 2100                      	AND		TEMP1, MDFLG														; ^
                                 
000225 3100                      	CPI		TEMP1, (1<<BROD1)													; Tachometer Mode		= (011)
000226 f031                      	BREQ	BROADCAST_TACHOMETER												; ^
                                 	
000227 3300                      	CPI		TEMP1, (1<<BROD2)|(1<<BROD1)										; Finishline Mode		= (111)
000228 f091                      	BREQ	BROADCAST_FINISHLINE												; ^
                                 
000229 3200                      	CPI		TEMP1, (1<<BROD2)													; Accelerometer Mode	= (101)
00022a f059                      	BREQ	BROADCAST_ACCELEROMETER												; ^
                                 
00022b d014                      	RCALL	BROADCAST_ALL														; Broadcast All			= (001)
                                 
00022c 9508                      	RET																			; Return
                                 
                                 BROADCAST_TACHOMETER:
                                 
00022d 9150 0060                 	LDS		TXREG, TACHOMETER_H													; Load & transmit HIGH byte of Tachometer data
00022f 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
000231 9150 0061                 	LDS		TXREG, TACHOMETER_L													; Load & transmit HIGH byte of Tachometer data
000233 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
000235 9508                      	RET																			; Return
                                 
                                 BROADCAST_ACCELEROMETER:
                                 
000236 9150 0073                 	LDS		TXREG, ACCELEROMETER												; Load & transmit Accelerometer data
000238 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
00023a 9508                      	RET																			; Return
                                 
                                 BROADCAST_FINISHLINE:
                                 	
00023b 9150 0070                 	LDS		TXREG, FINISHLINE													; Load & transmit Finishline data
00023d 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
00023f 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL:
                                 
000240 9100 0061                 	LDS		TEMP1, TACHOMETER_L													; Load current & recent Tachometer (LOW) value
000242 9110 0076                 	LDS		TEMP2, TACHOMETER_L_PREV											;
                                 
000244 1701                      	CP		TEMP1, TEMP2														; Compare and only transmit if value has changed
000245 f409                      	BRNE	BROADCAST_ALL_SEND													; ^
                                 
000246 9508                      	RET																			; Return
                                 
                                 BROADCAST_ALL_SEND:
                                 
000247 9300 0076                 	STS		TACHOMETER_L_PREV, TEMP1											; Update recent Tachometer (LOW) value
                                 
000249 dfe3                      	RCALL	BROADCAST_TACHOMETER												; Broadcast Tachometer
00024a dfeb                      	RCALL	BROADCAST_ACCELEROMETER												; Broadcast Accelerometer
                                 
00024b 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> COMMUNICATION PROTOCOL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > USART
                                 
                                 SERIAL_READ:
                                 	
00024c 9b5f                      	SBIS	UCSRA, RXC															; Wait for Recieve (RXC) Flag
00024d cffe                      	RJMP	SERIAL_READ															; ^
                                 
00024e b14c                      	IN		RXREG, UDR															; Read data into Reception Register
00024f 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
000251 9508                      	RET																			; Return
                                 
                                 SERIAL_WRITE:
                                 	
000252 9b5d                      	SBIS	UCSRA, UDRE															; Wait for Empty Transmit Buffer (UDRE) Flag
000253 cffe                      	RJMP	SERIAL_WRITE														; ^
                                 
000254 b95c                      	OUT		UDR, TXREG															; Write data from Transmission Register
                                 
000255 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > TELEGRAM PARSER
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support recent Z pointer from SRAM!
                                 
                                 TELEGRAM_CHECK:
                                 
000256 9b5f                      	SBIS	UCSRA, RXC															; Return if no data in USART reception buffer
000257 9508                      	RET																			; ^
                                 
000258 b14c                      	IN		RXREG, UDR															; Read data into Reception Register
000259 9340 006b                 	STS		SERIAL_RX, RXREG													; Store data in SRAM
                                 
                                 TELEGRAM_PARSER:
                                 	
00025b 9100 0066                 	LDS		TEMP1, TEL_STEP														; Load, increment & store Telegram Step Counter
00025d 9503                      	INC		TEMP1																; ^
00025e 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
000260 3001                      	CPI		TEMP1, 1															; Setup Telegram Parser if Step = 1
000261 f041                      	BREQ	TELEGRAM_PARSE_SETUP												; ^
                                 
000262 3004                      	CPI		TEMP1, 4															; Execute Telegram if Step = 4
000263 f089                      	BREQ	TELEGRAM_EXECUTE													; ^
                                 
000264 3002                      	CPI		TEMP1, 2															; Parse (TYPE) if Step = 2
000265 f041                      	BREQ	TELEGRAM_JUMP														; ^
                                 
000266 95ea                      	DEC		ZL																	; Offset Z pointer (-1) to parse TYPE (0x00_XX)
                                 
000267 3003                      	CPI		TEMP1, 3															; Parse (COMMAND) if Step = 3
000268 f029                      	BREQ	TELEGRAM_JUMP														; ^
                                 
                                 TELEGRAM_PARSER_ESC:
                                 	
000269 9508                      	RET
                                 
                                 TELEGRAM_PARSE_SETUP:
                                 
00026a e0f0                      	LDI		ZH, HIGH(COMMANDS*2)												; Reset Z Pointer to COMMANDS jump table
00026b e5e0                      	LDI 	ZL,  LOW(COMMANDS*2)												; ^
                                 
00026c 95e3                      	INC		ZL																	; Offset Z pointer (+1) to parse COMMAND (0xXX_00)
                                 
00026d cfed                      	RJMP	TELEGRAM_PARSER														; Return
                                 
                                 TELEGRAM_JUMP:
                                 
00026e 9634                      	ADIW	ZH:ZL, 4															; Increment Z Pointer (by 4)
                                 
00026f 9104                      	LPM		TEMP1, Z															; Load the matching duty cycle
                                 
000270 3e0e                      	CPI		TEMP1, 0xEE															; Reset everything if out of table bounds
000271 f0b1                      	BREQ	TELEGRAM_ERROR														; ^
                                 
000272 1704                      	CP		TEMP1, RXREG														; Find match in jump table
000273 f3a9                      	BREQ	TELEGRAM_PARSER_ESC													; ^
                                 
000274 cff9                      	RJMP	TELEGRAM_JUMP														; Repeat
                                 
                                 TELEGRAM_EXECUTE:
                                 
000275 9632                      	ADIW	ZH:ZL, 2															; Point at & read LOW of Table address
000276 9104                      	LPM		TEMP1, Z															; ^
                                 
000277 9631                      	ADIW	ZH:ZL, 1															; Point at & read HIGH of Table address
000278 9114                      	LPM		TEMP2, Z															; ^
                                 
000279 2fe0                      	MOV		ZL, TEMP1															; Load Z Pointer
00027a 2ff1                      	MOV		ZH, TEMP2															; ^
                                 
00027b 9340 0068                 	STS		RECENT_DAT, RXREG													; Store recieved data in SRAM
                                 
00027d 2d01
00027e 6008
00027f 2e10                      	SFLG	FNFLG, CMDPD														; Set CMDPD flag in FNFLG
                                 
                                 TELEGRAM_RESET:
                                 	
000280 2700                      	CLR		TEMP1																; Reset & store parse step counter
000281 9300 0066                 	STS		TEL_STEP, TEMP1														; ^
                                 
000283 9508                      	RET																			; Return
                                 
                                 TELEGRAM_CLRBUFFER:
                                 
000284 b10c                      	IN		TEMP1, UDR															; Empty buffer
000285 995f                      	SBIC	UCSRA, RXC															; ^
000286 cffd                      	RJMP	TELEGRAM_CLRBUFFER													; ^
                                 
000287 9508                      	RET																			; Return
                                 
                                 TELEGRAM_ERROR:
                                 
000288 2744                      	CLR		RXREG																; Clear reception register
                                 
000289 2d01
00028a 7f07
00028b 2e10                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
00028c dff3                      	RCALL	TELEGRAM_RESET														; Reset parse step counter
00028d dff6                      	RCALL	TELEGRAM_CLRBUFFER													; Clear reception buffer
                                 	
00028e 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > COMMANDS
                                 
                                 EXECUTE_COMMAND:
                                 
00028f 2d01
000290 7f07
000291 2e10                      	CFLG	FNFLG, CMDPD														; Clear CMDPD flag in FNFLG
                                 
000292 9509                      	ICALL																		; Call function (address) of Z-pointer
                                 
000293 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 BROADCAST_SET:
                                 
000294 2d00                      	MOV		TEMP1, MDFLG														; Load & reset current Broadcast Flags in Mode Register
000295 7c07                      	ANDI	TEMP1, 0b11000111													; ^
                                 
000296 9110 0068                 	LDS		TEMP2, RECENT_DAT													; Load & apply recieved Broadcast Flags
000298 2b01                      	OR		TEMP1, TEMP2														; ^
                                 
000299 2e00                      	MOV		MDFLG, TEMP1														; Save new mode flags
00029a 9200 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
00029c 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_SET:
                                 
00029d 2d00
00029e 6400
00029f 2e00                      	SFLG	MDFLG, MAP															; Set MAP flag in MDFLG
                                 
0002a0 9200 006f                 	STS		MODE_FLG, MDFLG														; Store new mode flags to SRAM
                                 
0002a2 e30c                      	LDI		TEMP1, MAPPING_SEEK_PWM												; Start vehicle with mapping seek PWM
0002a3 9300 0068                 	STS		RECENT_DAT, TEMP1													; ^	
0002a5 940e 02c9                 	CALL	SET_MOTOR_PWM														; ^
                                 
0002a7 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_SET:
                                 
0002a8 940e 0182                 	CALL	TRAJECTORY_COMPILE													; Compile Trajectory
                                 
0002aa 9508                      	RET
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 MAPPING_GET:
                                 
                                 	// If MAPP > 512 bytes, then buffer will overflow in MatLab
                                 	
0002ab e0d2                      	LDI		YH, HIGH(MAPP_TABLE)												; Initialize Y Pointer
0002ac e0c0                      	LDI		YL,  LOW(MAPP_TABLE)												; ^
                                 
                                 MAPPING_GET_LOOP:
                                 
0002ad 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
0002ae 9119                      	LD		TEMP2, Y+															; ^
                                 
0002af 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
0002b0 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
0002b2 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
0002b3 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
0002b5 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
0002b6 f009                      	BREQ	MAPPING_GET_ESC														; ^
                                 
0002b7 cff5                      	RJMP	MAPPING_GET_LOOP													; Loop
                                 
                                 MAPPING_GET_ESC:
                                 
0002b8 9508                      	RET																			; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 
                                 TRAJECTORY_GET:
                                 
                                 	// If TRAJ > 512 bytes, then buffer will overflow in MatLab
                                 	
0002b9 940e 0182                 	CALL	TRAJECTORY_COMPILER_SETUP											; Compile Trajectory
                                 	
0002bb e0d3                      	LDI		YH, HIGH(TRAJ_TABLE)												; Initialize Y Pointer
0002bc e0c0                      	LDI		YL,  LOW(TRAJ_TABLE)												; ^
                                 
                                 TRAJECTORY_GET_LOOP:
                                 
0002bd 9109                      	LD		TEMP1, Y+															; Load mapping values (HIGH & LOW)
0002be 9119                      	LD		TEMP2, Y+															; ^
                                 
0002bf 2f50                      	MOV		TXREG, TEMP1														; Transmit HIGH byte of mapping
0002c0 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
0002c2 2f51                      	MOV		TXREG, TEMP2														; Transmit LOW byte of mapping
0002c3 940e 0252                 	CALL	SERIAL_WRITE														; ^
                                 
0002c5 3f0f                      	CPI		TEMP1, 0xFF															; Escape if EoT has been reached
0002c6 f009                      	BREQ	TRAJECTORY_GET_ESC													; ^
                                 
0002c7 cff5                      	RJMP	TRAJECTORY_GET_LOOP													; Loop
                                 
                                 TRAJECTORY_GET_ESC:
                                 
0002c8 9508                      	RET																			; Return
                                 
                                 ; ____________________________________________________________________________________________________________________________________________________
                                 ; >> DEVICE (RACECAR) CONTROL
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOTOR CONTROL
                                 
                                 SET_MOTOR_PWM:
                                 
0002c9 e60a                      	LDI		TEMP1, 0x6A															; Initialize Waveform Generator (Timer2) (0110_1010)
0002ca bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
0002cb 9120 0068                 	LDS		TEMP3, RECENT_DAT													; Load recent recieved telegram data from SRAM
0002cd 9320 006d                 	STS		DUTY_CYCLE, TEMP3													; Store loaded Duty Cycle in SRAM
                                 	
0002cf 2322                      	TST		TEMP3																; Check if recieved Duty Cycle is 0
0002d0 f029                      	BREQ	SET_MOTOR_BREAK														; Break vehicle if true
                                 
0002d1 9894                      	CBI		PORTD, PD4															; Disable MOSFET Brake
                                 
0002d2 940e 0347                 	CALL	DELAY_100uS															; Wait for 100 ?s
                                 
0002d4 bd23                      	OUT		OCR2, TEMP3															; Set Duty Cycle (PWM) (0-255) on Timer2
                                 
                                 SET_MOTOR_PWM_ESC:
                                 
0002d5 9508                      	RET																			; Return
                                 
                                 SET_MOTOR_BREAK:
                                 
0002d6 e000                      	LDI		TEMP1, 0x00															; Disable Timer2 (PWM)
0002d7 bd05                      	OUT		TCCR2, TEMP1														; ^
                                 
0002d8 9897                      	CBI 	PORTD, PD7															; Clear PD7 of PORTD
                                 		
0002d9 940e 0347                 	CALL	DELAY_100uS															; Wait for 100 us
                                 
0002db 9a94                      	SBI		PORTD, PD4															; Enable MOSFET Brake
                                 
0002dc cff8                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 SET_MOTOR_MAX:
                                 
0002dd 9a97                      	SBI 	PORTD, PD7															; Set PD7 of PORTD
0002de cff6                      	RJMP	SET_MOTOR_PWM_ESC													; Return
                                 
                                 ;  _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _ _
                                 ;  > MOVING AVERAGE
                                 
                                 	// !#!#!#!
                                 	// Needs to be changed to support an arbitrary SRAM Location!
                                 
                                 MOVAVG:
                                 
0002df 91b0 0071                 	LDS		XH, MOVAVG_RECENT_XH												; Load recent MOVAVG X pointer
0002e1 91a0 0072                 	LDS		XL, MOVAVG_RECENT_XL												; ^
                                 
0002e3 9100 0064                 	LDS		TEMP1, ADC_H														; Insert ADC_val from high because of ADLAR
0002e5 930d                      	ST		X+, TEMP1															; Save ADC_H to SRAM pointer X
                                 
0002e6 34a0                      	CPI		XL, MOVAVG_SIZE														;
                                 	//CPI		XL, LOW(MOVAVG_TABLE_END)										; Check if Pointer should be reset
0002e7 f409                      	BRNE	MOVAVG_SKIP_RESET													; ^
                                 	//CPI		XH, HIGH(MOVAVG_TABLE_END)										; Check if Pointer should be reset
                                 	//BRNE	MOVAVG_SKIP_RESET													; ^
0002e8 d005                      	RCALL	MOVAVG_POINTER_RESET												; ^
                                 
                                 MOVAVG_SKIP_RESET:
                                 	
                                 	//STS		MOVAVG_RECENT_XH, XH												; - skal med
0002e9 93a0 0072                 	STS		MOVAVG_RECENT_XL, XL												; -
                                 
0002eb d00e                      	RCALL	MOVAVG_ADD															; Do Moving Average Addition
0002ec d018                      	RCALL	MOVAVG_DIVIDE														; Do Moving Average Division
                                 
0002ed 9508                      	RET																			; Return
                                 
                                 MOVAVG_POINTER_RESET:
                                 
0002ee e0b1                      	LDI		XH, HIGH(MOVAVG_TABLE)												; Load reset values into X Pointer
0002ef e0a0                      	LDI		XL,  LOW(MOVAVG_TABLE)												; ^
                                 
0002f0 9508                      	RET																			; Return
                                 
                                 MOVAVG_SRAM_SETUP:
                                 
0002f1 e400                      	LDI		TEMP1, MOVAVG_SIZE													; Load size of Moving Average filter into register
                                 
0002f2 93b0 0071                 	STS		MOVAVG_RECENT_XH, XH												; Store location of X pointer into SRAM
0002f4 93a0 0072                 	STS		MOVAVG_RECENT_XL, XL												; ^
                                 
                                 MOVAVG_SRAM_SETUP_LOOP:
                                 	
0002f6 930d                      	ST		X+, TEMP1															; Set all values of SRAM to default value
                                 
0002f7 34a0                      	CPI		XL, LOW(MOVAVG_TABLE_END)											; Check if reached end of table.
0002f8 f7e9                      	BRNE	MOVAVG_SRAM_SETUP_LOOP												; ^
                                 	//CPI	XH, HIGH(MOVAVG_TABLE_END)						16BIT COMPARE!! CPC	; Check if reached end of table.
                                 	//BRNE	MOVAVG_SRAM_SETUP_LOOP												; ^
                                 
0002f9 9508                      	RET																			; Return
                                 
                                 MOVAVG_ADD:
                                 	
0002fa 2700                      	CLR		TEMP1																; Reset Temporary Register
0002fb 2711                      	CLR		TEMP2																; ^
0002fc 2722                      	CLR		TEMP3																; ^
                                 
0002fd dff0                      	RCALL	MOVAVG_POINTER_RESET												; Reset X Pointer
                                 
                                 MOVAVG_ADD_LOOP:
                                 	
0002fe 910d                      	LD		TEMP1, X+ 															; Load value from X pointer location.
0002ff 0f10                      	ADD		TEMP2, TEMP1														; Add values
                                 
                                 	// Can be changed to SBIC, SREG ..
                                 	
                                 	//SBRC	SREG, 0
000300 f408                      	BRCC	MOVAVG_ADD_SKIP_CARRY												; Branch if carry is not set - skal slettes
000301 9523                      	INC		TEMP3																; ^
                                 
                                 MOVAVG_ADD_SKIP_CARRY:															;skal slettes!
                                 	
000302 34a0                      	CPI		XL, LOW(MOVAVG_TABLE_END)											; Check if reached end of table
000303 f7d1                      	BRNE	MOVAVG_ADD_LOOP														; 
                                 	//CPI		XH, HIGH(MOVAVG_TABLE_END) THIS IS WRONG SHOULD BE CPC 16bit if ever moved!
                                 	//BRNE	MOVAVG_ADD_LOOP
                                 
000304 9508                      	RET																			; Return
                                 
                                 MOVAVG_DIVIDE:
                                 
000305 e006                      	LDI		TEMP1, MOVAVG_DIVS													; Load number of Divisions
                                 
                                 MOVAVG_DIVIDE_LOOP:
                                 
000306 9525                      	ASR		TEMP3																; TEMP3 IS HIGH
000307 9517                      	ROR		TEMP2																; TEMP2 IS LOW
                                 
000308 950a                      	DEC		TEMP1																; Perform 16 bit divison until done
000309 f7e1                      	BRNE	MOVAVG_DIVIDE_LOOP													; ^
                                 
00030a 9310 0073                 	STS		ACCELEROMETER, TEMP2												; Save value of division into SRAM
                                 
00030c 9508                      	RET
                                 
