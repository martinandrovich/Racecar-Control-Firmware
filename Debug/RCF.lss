000032 ec0f                      	LDI		TEMP1, BAUDRATE								; Set Transmission Rate
000033 b909                      	OUT		UBRRL, TEMP1								; ^
000034 e000                      	LDI		TEMP1, 0x00									; ^
000035 bd00                      	OUT		UBRRH, TEMP1								; ^
                                 
000036 e002                      	LDI		TEMP1, 0x02									; Clear all Error Flags + Enable DoubleMode
000037 b90b                      	OUT		UCSRA, TEMP1								; ^
                                 
000038 e108                      	LDI		TEMP1, (1<<RXEN) | (1<<TXEN)				; Enable Transmission & Reception
000039 b90a                      	OUT		UCSRB, TEMP1								; ^
                                 
00003a e806                      	LDI		TEMP1, (1<<URSEL) | (3<<UCSZ0)				; Set Frame Format (8, N, 1)
00003b bd00                      	OUT		UCSRC, TEMP1								; ^
                                 
00003c e040                      	LDI		RXREG, 0x00									; Reset Reception Register
00003d e050                      	LDI		TXREG, 0x00									; Reset Transmission Register
                                 
