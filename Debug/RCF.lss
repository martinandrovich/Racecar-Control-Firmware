000044 e100                      	LDI		TEMP1, (1<<OCIE1A)							;Enable Timer1 Compare Match INterrupt
000045 bf09                      	OUT		TIMSK, TEMP1								;
                                 
000046 e000                      	LDI		TEMP1,	00									;Set Default
000047 bd0f                      	OUT		TCCR1A, TEMP1								;
                                 
000048 e00b                      	LDI		TEMP1, (1<<CS11)|(1<<CS10)|(1<<WGM12)		;Set 64 Prescalar, CTC-MODE, 62.500 TIMER, since it is very close
000049 bd0e                      	OUT		TCCR1B, TEMP1								;
                                 
00004a ef04                      	LDI		TEMP1, HIGH(HERTZTIMER1)					;Set this to 62500-1, since 16.000.000 / ( 4 * 64) 4 Hertz
00004b bd1b                      	OUT		OCR1AH, TEMP2								;
                                 
00004c e203                      	LDI		TEMP1, LOW(HERTZTIMER1)						;Set this to 62500-1, since 16.000.000 / ( 4 * 64) 4 Hertz
00004d bd1a                      	OUT		OCR1AL, TEMP2								;
                                 
00004e e004                      	LDI		TEMP1, (1<<TOV1)							; Enable Timer1
00004f bf08                      	OUT		TIFR, TEMP1									;^
                                 
