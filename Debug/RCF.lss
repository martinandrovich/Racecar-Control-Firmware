00006b b90a                      	OUT		UCSRB, R16						; ^
                                 
00006c e806                      	LDI		R16, (1<<URSEL) | (3<<UCSZ0)	; Set Frame Format (8, N, 1)
00006d bd00                      	OUT		UCSRC, R16						; ^
                                 
                                 	; Init Port D
                                 
00006e ef0f                      	LDI		R16, 0xFF						; Set PORTD as Output
00006f 9a8f                      	SBI		DDRD, PD7						; ^
                                 
000070 e040                      	LDI		RXREG, 0x00						; Reset Reception Register
