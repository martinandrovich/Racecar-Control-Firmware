00020f e820                      	LDI		TEMP3, (1<<MSB)														; Extract & Remove MSB of Trajectory Tachometer value
000210 2329                      	AND		TEMP3, TEMPWH														; ^
000211 779f                      	CBR		TEMPWH, (1<<MSB)													; ^
                                 
